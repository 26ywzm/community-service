{
  "name": "community",
  "version": "1.0.0",
  "scripts": {
    "start": "run-p start:client start:server",
    "start:client": "cd community-service && npm run serve",
    "start:server": "cd server && node app.js",
    "install:all": "run-s install:client install:server",
    "install:client": "cd community-service && npm install",
    "install:server": "cd server && npm install"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
