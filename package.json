{
  "name": "community",
  "version": "1.0.0",
  "scripts": {
    "start": "npx run-p start:client start:server",
    "start:client": "cd community-service && npm run serve",
    "start:server": "cd server && node app.js",
    "install:all": "npx run-s install:client install:server",
    "install:client": "cd community-service && npm install",
    "install:server": "cd server && npm install"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.0",
    "npm-run-all": "^4.1.5",
    "unplugin-auto-import": "^0.18.5",
    "unplugin-vue-components": "^0.27.4",
    "vite": "^5.4.11"
  }
}
