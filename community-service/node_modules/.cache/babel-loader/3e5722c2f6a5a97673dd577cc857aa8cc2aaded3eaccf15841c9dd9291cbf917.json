{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\n// import router from './router';\n\nexport default {\n  name: 'UserLogin',\n  data() {\n    return {\n      username: '',\n      password: ''\n    };\n  },\n  methods: {\n    async login() {\n      try {\n        const response = await axios.post('http://localhost:3000/api/login', {\n          username: this.username,\n          password: this.password\n        });\n        alert(response.data.message);\n        // 登录成功后的操作，如保存token，跳转页面等\n        localStorage.setItem('authToken', response.data.token); // 保存 token\n        this.$router.push('/profile'); // 登录成功后跳转到“我的”页面\n      } catch (error) {\n        alert('登录失败，请检查用户名和密码。');\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","username","password","methods","login","response","post","alert","message","localStorage","setItem","token","$router","push","error"],"sources":["C:\\Users\\拯救者\\Desktop\\text\\community-service\\src\\components\\UserLogin.vue"],"sourcesContent":["<template>\r\n    <div>\r\n      <h2>用户登录</h2>\r\n      <form @submit.prevent=\"login\">\r\n        <div>\r\n          <label>用户名：</label>\r\n          <input type=\"text\" v-model=\"username\" required />\r\n        </div>\r\n        <div>\r\n          <label>密码：</label>\r\n          <input type=\"password\" v-model=\"password\" required />\r\n        </div>\r\n        <button type=\"submit\">登录</button>\r\n      </form>\r\n    </div>\r\n  </template>\r\n  \r\n  <script>\r\n  import axios from 'axios';\r\n  // import router from './router';\r\n\r\n  export default {\r\n    name: 'UserLogin',\r\n    data() {\r\n      return {\r\n        username: '',\r\n        password: '',\r\n      };\r\n    },\r\n    methods: {\r\n      async login() {\r\n        try {\r\n          const response = await axios.post('http://localhost:3000/api/login', {\r\n            username: this.username,\r\n            password: this.password,\r\n          });\r\n          alert(response.data.message);\r\n          // 登录成功后的操作，如保存token，跳转页面等\r\n          localStorage.setItem('authToken', response.data.token); // 保存 token\r\n          this.$router.push('/profile'); // 登录成功后跳转到“我的”页面\r\n        } catch (error) {\r\n          alert('登录失败，请检查用户名和密码。');\r\n        }\r\n      },\r\n    },\r\n  };\r\n  \r\n  </script>\r\n  \r\n  <style scoped>\r\n  /* 添加样式 */\r\n  </style>\r\n  "],"mappings":";AAkBE,OAAOA,KAAI,MAAO,OAAO;AACzB;;AAEA,eAAe;EACbC,IAAI,EAAE,WAAW;EACjBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,EAAE;MACZC,QAAQ,EAAE;IACZ,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,KAAKA,CAAA,EAAG;MACZ,IAAI;QACF,MAAMC,QAAO,GAAI,MAAMP,KAAK,CAACQ,IAAI,CAAC,iCAAiC,EAAE;UACnEL,QAAQ,EAAE,IAAI,CAACA,QAAQ;UACvBC,QAAQ,EAAE,IAAI,CAACA;QACjB,CAAC,CAAC;QACFK,KAAK,CAACF,QAAQ,CAACL,IAAI,CAACQ,OAAO,CAAC;QAC5B;QACAC,YAAY,CAACC,OAAO,CAAC,WAAW,EAAEL,QAAQ,CAACL,IAAI,CAACW,KAAK,CAAC,EAAE;QACxD,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,UAAU,CAAC,EAAE;MACjC,EAAE,OAAOC,KAAK,EAAE;QACdP,KAAK,CAAC,iBAAiB,CAAC;MAC1B;IACF;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}