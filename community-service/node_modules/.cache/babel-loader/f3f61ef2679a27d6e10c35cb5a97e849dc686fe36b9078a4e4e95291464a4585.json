{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nimport { ElMessage } from 'element-plus';\nimport { adminAuth } from '@/mixins/adminAuth';\nconst API = process.env.VUE_APP_BASE_URL;\nexport default {\n  mixins: [adminAuth],\n  data() {\n    return {\n      votes: [],\n      loading: false\n    };\n  },\n  mounted() {\n    this.fetchVotes();\n  },\n  methods: {\n    async fetchVotes() {\n      this.loading = true;\n      try {\n        const response = await axios.get(`${API}/api/canteen/votes`, {\n          headers: {\n            'Authorization': `Bearer ${localStorage.getItem('authToken')}`\n          }\n        });\n        this.votes = response.data;\n      } catch (error) {\n        console.error('获取投票失败:', error);\n        ElMessage.error('获取投票列表失败');\n      } finally {\n        this.loading = false;\n      }\n    },\n    confirmDelete(vote) {\n      if (confirm(`确定要删除投票\"${vote.title}\"吗？此操作不可撤销。`)) {\n        this.deleteVote(vote.id);\n      }\n    },\n    async deleteVote(voteId) {\n      try {\n        await axios.delete(`${API}/api/canteen/votes/${voteId}`, {\n          headers: {\n            'Authorization': `Bearer ${localStorage.getItem('authToken')}`\n          }\n        });\n        ElMessage.success('投票删除成功');\n        this.fetchVotes();\n      } catch (error) {\n        console.error('删除投票失败:', error);\n        ElMessage.error('删除投票失败');\n      }\n    },\n    createNewVote() {\n      this.$router.push('/create-vote');\n    }\n  }\n};","map":{"version":3,"names":["axios","ElMessage","adminAuth","API","process","env","VUE_APP_BASE_URL","mixins","data","votes","loading","mounted","fetchVotes","methods","response","get","headers","localStorage","getItem","error","console","confirmDelete","vote","confirm","title","deleteVote","id","voteId","delete","success","createNewVote","$router","push"],"sources":["C:\\Users\\拯救者\\Desktop\\text\\community-service\\src\\components\\AdminManageVotes.vue"],"sourcesContent":["<template>\n  <div>\n    <h2>管理投票</h2>\n    <div v-if=\"loading\" class=\"loading\">\n      <span>加载中...</span>\n    </div>\n    <div v-else>\n      <div v-for=\"vote in votes\" :key=\"vote.id\" class=\"vote-item\">\n        <div class=\"vote-info\">\n          <h3>{{ vote.title }}</h3>\n          <p>{{ vote.description }}</p>\n        </div>\n        <div class=\"vote-actions\">\n          <button class=\"delete-btn\" @click=\"confirmDelete(vote)\">删除投票</button>\n        </div>\n      </div>\n      <button class=\"create-btn\" @click=\"createNewVote\">创建新投票</button>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios';\nimport { ElMessage } from 'element-plus';\nimport { adminAuth } from '@/mixins/adminAuth';\nconst API = process.env.VUE_APP_BASE_URL;\n\nexport default {\n  mixins: [adminAuth],\n  data() {\n    return {\n      votes: [],\n      loading: false,\n    };\n  },\n  mounted() {\n    this.fetchVotes();\n  },\n  methods: {\n    async fetchVotes() {\n      this.loading = true;\n      try {\n        const response = await axios.get(`${API}/api/canteen/votes`, {\n          headers: {\n            'Authorization': `Bearer ${localStorage.getItem('authToken')}`\n          }\n        });\n        this.votes = response.data;\n      } catch (error) {\n        console.error('获取投票失败:', error);\n        ElMessage.error('获取投票列表失败');\n      } finally {\n        this.loading = false;\n      }\n    },\n\n    confirmDelete(vote) {\n      if (confirm(`确定要删除投票\"${vote.title}\"吗？此操作不可撤销。`)) {\n        this.deleteVote(vote.id);\n      }\n    },\n\n    async deleteVote(voteId) {\n      try {\n        await axios.delete(`${API}/api/canteen/votes/${voteId}`, {\n          headers: {\n            'Authorization': `Bearer ${localStorage.getItem('authToken')}`\n          }\n        });\n        ElMessage.success('投票删除成功');\n        this.fetchVotes();\n      } catch (error) {\n        console.error('删除投票失败:', error);\n        ElMessage.error('删除投票失败');\n      }\n    },\n\n    createNewVote() {\n      this.$router.push('/create-vote');\n    },\n  },\n};\n</script>\n\n<style scoped>\n.loading {\n  text-align: center;\n  padding: 20px;\n  color: #666;\n}\n\n.vote-item {\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  padding: 15px;\n  margin-bottom: 15px;\n  border: 1px solid #e8e8e8;\n  border-radius: 4px;\n  background-color: #fff;\n  transition: all 0.3s;\n}\n\n.vote-item:hover {\n  box-shadow: 0 2px 8px rgba(0,0,0,0.09);\n}\n\n.vote-info {\n  flex: 1;\n}\n\n.vote-info h3 {\n  margin: 0 0 8px 0;\n  color: #333;\n}\n\n.vote-info p {\n  margin: 0;\n  color: #666;\n}\n\n.vote-actions {\n  margin-left: 20px;\n}\n\n.delete-btn {\n  padding: 6px 15px;\n  background-color: #ff4d4f;\n  color: white;\n  border: none;\n  border-radius: 4px;\n  cursor: pointer;\n  transition: all 0.3s;\n}\n\n.delete-btn:hover {\n  background-color: #ff7875;\n}\n\n.create-btn {\n  margin-top: 20px;\n  padding: 8px 20px;\n  background-color: #1890ff;\n  color: white;\n  border: none;\n  border-radius: 4px;\n  cursor: pointer;\n  transition: all 0.3s;\n}\n\n.create-btn:hover {\n  background-color: #40a9ff;\n}\n</style>"],"mappings":";AAsBA,OAAOA,KAAI,MAAO,OAAO;AACzB,SAASC,SAAQ,QAAS,cAAc;AACxC,SAASC,SAAQ,QAAS,oBAAoB;AAC9C,MAAMC,GAAE,GAAIC,OAAO,CAACC,GAAG,CAACC,gBAAgB;AAExC,eAAe;EACbC,MAAM,EAAE,CAACL,SAAS,CAAC;EACnBM,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,OAAO,EAAE;IACX,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,UAAU,CAAC,CAAC;EACnB,CAAC;EACDC,OAAO,EAAE;IACP,MAAMD,UAAUA,CAAA,EAAG;MACjB,IAAI,CAACF,OAAM,GAAI,IAAI;MACnB,IAAI;QACF,MAAMI,QAAO,GAAI,MAAMd,KAAK,CAACe,GAAG,CAAC,GAAGZ,GAAG,oBAAoB,EAAE;UAC3Da,OAAO,EAAE;YACP,eAAe,EAAE,UAAUC,YAAY,CAACC,OAAO,CAAC,WAAW,CAAC;UAC9D;QACF,CAAC,CAAC;QACF,IAAI,CAACT,KAAI,GAAIK,QAAQ,CAACN,IAAI;MAC5B,EAAE,OAAOW,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,SAAS,EAAEA,KAAK,CAAC;QAC/BlB,SAAS,CAACkB,KAAK,CAAC,UAAU,CAAC;MAC7B,UAAU;QACR,IAAI,CAACT,OAAM,GAAI,KAAK;MACtB;IACF,CAAC;IAEDW