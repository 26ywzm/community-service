{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  data() {\n    return {\n      isMobile: false,\n      isNavHidden: false,\n      lastScrollTop: 0,\n      activeIndex: '1'\n    };\n  },\n  // computed: {\n  //   profilePath() {\n  //     const token = localStorage.getItem('authToken');\n  //     return token ? '/profile' : '/login';\n  //   }\n  // },\n  mounted() {\n    this.checkDevice();\n    window.addEventListener('resize', this.checkDevice);\n    window.addEventListener('scroll', this.handleScroll);\n  },\n  methods: {\n    checkDevice() {\n      this.isMobile = window.innerWidth <= 768;\n    },\n    handleScroll() {\n      const currentScrollTop = window.pageYOffset || document.documentElement.scrollTop;\n      this.isNavHidden = currentScrollTop > this.lastScrollTop && currentScrollTop > 100;\n      this.lastScrollTop = currentScrollTop;\n    },\n    handleProfileClick() {\n      // 检查是否已登录\n      const token = localStorage.getItem('authToken');\n      if (!token) {\n        // 如果未登录，跳转到登录页面\n        this.$router.push('/login');\n      }\n    }\n  }\n};","map":{"version":3,"names":["data","isMobile","isNavHidden","lastScrollTop","activeIndex","mounted","checkDevice","window","addEventListener","handleScroll","methods","innerWidth","currentScrollTop","pageYOffset","document","documentElement","scrollTop","handleProfileClick","token","localStorage","getItem","$router","push"],"sources":["C:\\Users\\拯救者\\Desktop\\text\\community-service\\src\\App.vue"],"sourcesContent":["<template>\n  <div id=\"app\">\n    <!-- 顶部导航栏（桌面和平板设备） -->\n    <el-menu v-if=\"!isMobile\" :default-active=\"activeIndex\" :class=\"{ 'nav-hidden': isNavHidden }\" mode=\"horizontal\" background-color=\"#444\"\n      text-color=\"#fff\" active-text-color=\"#1e90ff\" class=\"top-menu\">\n      <div class=\"logo\">社区服务</div>\n      <div class=\"menu-right\">\n        <el-menu-item index=\"1\">\n          <router-link to=\"/\">首页</router-link>\n        </el-menu-item>\n        <el-menu-item index=\"2\">\n          <router-link to=\"/discover\">发现</router-link>\n        </el-menu-item>\n        <el-menu-item index=\"3\">\n          <router-link to=\"/profile\">我的</router-link>\n        </el-menu-item>\n      </div>\n    </el-menu>\n\n    <!-- 路由视图，展示每个页面的内容 -->\n    <div :class=\"{ 'content-padding': !isNavHidden && !isMobile }\">\n      <router-view />\n    </div>\n\n    <!-- 底部导航栏（手机设备） -->\n    <van-tabbar v-if=\"isMobile\" route>\n      <van-tabbar-item replace to=\"/\" icon=\"home-o\">\n        首页\n      </van-tabbar-item>\n      <van-tabbar-item replace to=\"/discover\" icon=\"apps-o\">\n        发现\n      </van-tabbar-item>\n      <van-tabbar-item :to=\"profilePath\" icon=\"user-o\" @click=\"handleProfileClick\">\n        我的\n      </van-tabbar-item>\n    </van-tabbar>\n  </div>\n</template>\n\n<script>\nexport default {\n  data() {\n    return {\n      isMobile: false,\n      isNavHidden: false,\n      lastScrollTop: 0,\n      activeIndex: '1',\n    };\n  },\n  // computed: {\n  //   profilePath() {\n  //     const token = localStorage.getItem('authToken');\n  //     return token ? '/profile' : '/login';\n  //   }\n  // },\n  mounted() {\n    this.checkDevice();\n    window.addEventListener('resize', this.checkDevice);\n    window.addEventListener('scroll', this.handleScroll);\n  },\n  methods: {\n    checkDevice() {\n      this.isMobile = window.innerWidth <= 768;\n    },\n    handleScroll() {\n      const currentScrollTop = window.pageYOffset || document.documentElement.scrollTop;\n      this.isNavHidden = currentScrollTop > this.lastScrollTop && currentScrollTop > 100;\n      this.lastScrollTop = currentScrollTop;\n    },\n    handleProfileClick() {\n      // 检查是否已登录\n      const token = localStorage.getItem('authToken');\n      if (!token) {\n        // 如果未登录，跳转到登录页面\n        this.$router.push('/login');\n      }\n    }\n  }\n};\n</script>\n\n<style scoped>\n.top-menu {\n  display: flex;\n  /* 启用弹性布局 */\n  align-items: center;\n  /* 垂直居中 */\n  justify-content: space-between;\n  /* 左右分布 */\n  padding: 0 20px;\n  /* 增加一些左右内边距 */\n}\n\n.logo {\n  font-size: 18px;\n  color: white;\n  font-weight: bold;\n}\n\n.menu-right {\n  display: flex;\n  /* 子项横向排列 */\n  gap: 20px;\n  /* 菜单项之间的间距 */\n}\n\n.menu-right a {\n  color: white;\n  text-decoration: none;\n}\n\n.menu-right a:hover {\n  color: #1e90ff;\n}\n\n.el-menu {\n  display: flex;\n  align-items: center;\n  position: fixed;\n  top: 0;\n  left: 0;\n  width: 100%;\n  z-index: 1000;\n  transition: top 0.3s;\n  box-shadow: 0 2px 5px rgba(0, 0, 0, 0.3);\n}\n\n.nav-hidden {\n  top: -70px;\n}\n\n.content-padding {\n  padding-top: 70px;\n  padding-bottom: 60px;\n}\n\n.toggle-button {\n  cursor: pointer;\n  color: white;\n}\n\n.van-tabbar {\n  position: fixed;\n  bottom: 0;\n  left: 0;\n  width: 100%;\n  z-index: 1000;\n  box-shadow: 0 -2px 5px rgba(0, 0, 0, 0.1);\n  background-color: #fff;\n}\n\n/* Tabbar图标大小自定义 */\n.van-icon {\n  margin-bottom: 4px;\n}\n\n/* 响应式设计 */\n@media (max-width: 768px) {\n  .van-icon {\n    margin-bottom: 4px;\n  }\n}\n\n.menu-right .router-link-active {\n  color: #1e90ff !important;\n}\n</style>"],"mappings":";AAwCA,eAAe;EACbA,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,KAAK;MACfC,WAAW,EAAE,KAAK;MAClBC,aAAa,EAAE,CAAC;MAChBC,WAAW,EAAE;IACf,CAAC;EACH,CAAC;EACD;EACA;EACA;EACA;EACA;EACA;EACAC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,WAAW,CAAC,CAAC;IAClBC,MAAM,CAACC,gBAAgB,CAAC,QAAQ,EAAE,IAAI,CAACF,WAAW,CAAC;IACnDC,MAAM,CAACC,gBAAgB,CAAC,QAAQ,EAAE,IAAI,CAACC,YAAY,CAAC;EACtD,CAAC;EACDC,OAAO,EAAE;IACPJ,WAAWA,CAAA,EAAG;MACZ,IAAI,CAACL,QAAO,GAAIM,MAAM,CAACI,UAAS,IAAK,GAAG;IAC1C,CAAC;IACDF,YAAYA,CAAA,EAAG;MACb,MAAMG,gBAAe,GAAIL,MAAM,CAACM,WAAU,IAAKC,QAAQ,CAACC,eAAe,CAACC,SAAS;MACjF,IAAI,CAACd,WAAU,GAAIU,gBAAe,GAAI,IAAI,CAACT,aAAY,IAAKS,gBAAe,GAAI,GAAG;MAClF,IAAI,CAACT,aAAY,GAAIS,gBAAgB;IACvC,CAAC;IACDK,kBAAkBA,CAAA,EAAG;MACnB;MACA,MAAMC,KAAI,GAAIC,YAAY,CAACC,OAAO,CAAC,WAAW,CAAC;MAC/C,IAAI,CAACF,KAAK,EAAE;QACV;QACA,IAAI,CAACG,O