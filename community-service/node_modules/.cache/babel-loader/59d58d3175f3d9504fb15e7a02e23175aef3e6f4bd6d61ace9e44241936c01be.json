n.edit-btn:hover {\n  background-color: #1976D2;\n}\n\n.delete-btn {\n  background-color: #f44336;\n  color: white;\n}\n\n.delete-btn:hover {\n  background-color: #e53935;\n}\n</style>\n"],"mappings":";;EACOA,KAAK,EAAC;AAAe;;EAInBA,KAAK,EAAC;AAAgB;;EAGlBA,KAAK,EAAC;AAAY;;EAIlBA,KAAK,EAAC;AAAY;;EAIlBA,KAAK,EAAC;AAAY;;EAIlBA,KAAK,EAAC;AAAY;;EAIlBA,KAAK,EAAC;AAAY;;EAIlBA,KAAK,EAAC;AAAc;;EACfC,IAAI,EAAC,QAAQ;EAACD,KAAK,EAAC;;;EA7BtCE,GAAA;EAqCqCF,KAAK,EAAC;;oBArC3C;;EAwCaA,KAAK,EAAC;AAAgB;;EAEtBA,KAAK,EAAC;AAAO;;EACbA,KAAK,EAAC;AAAa;;EAEnBA,KAAK,EAAC;AAAmB;oBA7CtC;oBAAA;;EAAAE,GAAA;AAAA;;uBACEC,mBAAA,CAqDM,OArDNC,UAqDM,G,4BApDJC,mBAAA,CAAa,YAAT,MAAI,QAGRA,mBAAA,CA4BM,OA5BNC,UA4BM,GA3BJD,mBAAA,CAA2C,YAAAE,gBAAA,CAApCC,KAAA,CAAAC,SAAS,0BAChBJ,mBAAA,CAyBO;IAzBAK,QAAM,EAAAC,MAAA,QAAAA,MAAA,MAPnBC,cAAA,CAAAC,MAAA,IAO6BL,KAAA,CAAAC,SAAS,GAAGK,QAAA,CAAAC,cAAc,KAAKD,QAAA,CAAAE,WAAW;IAAIhB,KAAK,EAAC;MACzEK,mBAAA,CAGM,OAHNY,UAGM,G,0BAFJZ,mBAAA,CAA8B;IAAvBa,GAAG,EAAC;EAAM,GAAC,MAAI,Q,gBACtBb,mBAAA,CAAoF;IAA7Ec,EAAE,EAAC,MAAM;IAAClB,IAAI,EAAC,MAAM;IAVtC,uBAAAU,MAAA,QAAAA,MAAA,MAAAE,MAAA,IAUgDL,KAAA,CAAAY,OAAO,CAACC,IAAI,GAAAR,MAAA;IAAES,QAAQ,EAAR,EAAQ;IAACC,WAAW,EAAC;gCAAnCf,KAAA,CAAAY,OAAO,CAACC,IAAI,E,KAEpDhB,mBAAA,CAGM,OAHNmB,UAGM,G,0BAFJnB,mBAAA,CAA6B;IAAtBa,GAAG,EAAC;EAAO,GAAC,IAAE,Q,gBACrBb,mBAAA,CAAwF;IAAjFc,EAAE,EAAC,OAAO;IAAClB,IAAI,EAAC,QAAQ;IAdzC,uBAAAU,MAAA,QAAAA,MAAA,MAAAE,MAAA,IAcmDL,KAAA,CAAAY,OAAO,CAACK,KAAK,GAAAZ,MAAA;IAAES,QAAQ,EAAR,EAAQ;IAACC,WAAW,EAAC;gCAApCf,KAAA,CAAAY,OAAO,CAACK,KAAK,E,KAExDpB,mBAAA,CAGM,OAHNqB,UAGM,G,0BAFJrB,mBAAA,CAAmC;IAA5Ba,GAAG,EAAC;EAAW,GAAC,MAAI,Q,gBAC3Bb,mBAAA,CAAsF;IAA/Ec,EAAE,EAAC,WAAW;IAAClB,IAAI,EAAC,MAAM;IAlB3C,uBAAAU,MAAA,QAAAA,MAAA,MAAAE,MAAA,IAkBqDL,KAAA,CAAAY,OAAO,CAACO,SAAS,GAAAd,MAAA;IAAEU,WAAW,EAAC;gCAA/Bf,KAAA,CAAAY,OAAO,CAACO,SAAS,E,KAE9DtB,mBAAA,CAGM,OAHNuB,UAGM,G,4BAFJvB,mBAAA,CAAoB,eAAb,OAAK,QACZA,mBAAA,CAAgE;IAAzDJ,IAAI,EAAC,MAAM;IAAE4B,QAAM,EAAAlB,MAAA,QAAAA,MAAA,UAAAmB,IAAA,KAAEhB,QAAA,CAAAiB,gBAAA,IAAAjB,QAAA,CAAAi