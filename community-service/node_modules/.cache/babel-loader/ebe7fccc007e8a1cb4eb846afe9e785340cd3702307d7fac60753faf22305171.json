{"ast":null,"code":"import axios from 'axios';\nconst BASE_URL = process.env.VUE_APP_BASE_URL;\nexport default {\n  data() {\n    return {\n      message: '' // 用户输入的留言内容\n    };\n  },\n  methods: {\n    async submitFeedback() {\n      const userId = localStorage.getItem('userId'); // 假设你已经存储了用户 ID\n\n      if (!this.message.trim()) {\n        alert('留言内容不能为空');\n        return;\n      }\n      try {\n        const response = await axios.post(`${BASE_URL}/api/canteen/feedback`, {\n          user_id: userId,\n          message: this.message\n        });\n        alert(response.data.message);\n        this.message = ''; // 清空留言内容\n      } catch (error) {\n        console.error('提交留言失败:', error);\n        alert('提交留言失败，请稍后重试');\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","BASE_URL","process","env","VUE_APP_BASE_URL","data","message","methods","submitFeedback","userId","localStorage","getItem","trim","alert","response","post","user_id","error","console"],"sources":["C:\\Users\\拯救者\\Desktop\\text\\community-service\\src\\components\\SubmitFeedback.vue"],"sourcesContent":["<template>\r\n    <div class=\"submit-feedback\">\r\n      <h2>提交您的建议或留言</h2>\r\n      <textarea v-model=\"message\" placeholder=\"请填写您的留言\" rows=\"4\" required></textarea>\r\n      <button @click=\"submitFeedback\">提交留言</button>\r\n    </div>\r\n  </template>\r\n  \r\n  <script>\r\n  import axios from 'axios';\r\n  const BASE_URL = process.env.VUE_APP_BASE_URL;\r\n  export default {\r\n    data() {\r\n      return {\r\n        message: '', // 用户输入的留言内容\r\n      };\r\n    },\r\n    methods: {\r\n      async submitFeedback() {\r\n        const userId = localStorage.getItem('userId'); // 假设你已经存储了用户 ID\r\n  \r\n        if (!this.message.trim()) {\r\n          alert('留言内容不能为空');\r\n          return;\r\n        }\r\n  \r\n        try {\r\n          const response = await axios.post(`${BASE_URL}/api/canteen/feedback`, {\r\n            user_id: userId,\r\n            message: this.message,\r\n          });\r\n          alert(response.data.message);\r\n          this.message = ''; // 清空留言内容\r\n        } catch (error) {\r\n          console.error('提交留言失败:', error);\r\n          alert('提交留言失败，请稍后重试');\r\n        }\r\n      },\r\n    },\r\n  };\r\n  </script>\r\n  \r\n  <style scoped>\r\n  .submit-feedback {\r\n    padding: 20px;\r\n  }\r\n  \r\n  textarea {\r\n    width: 100%;\r\n    margin-bottom: 10px;\r\n    padding: 10px;\r\n  }\r\n  \r\n  button {\r\n    padding: 10px 20px;\r\n    background-color: #4CAF50;\r\n    color: white;\r\n    border: none;\r\n    cursor: pointer;\r\n  }\r\n  \r\n  button:hover {\r\n    background-color: #45a049;\r\n  }\r\n  </style>\r\n  "],"mappings":"AASE,OAAOA,KAAI,MAAO,OAAO;AACzB,MAAMC,QAAO,GAAIC,OAAO,CAACC,GAAG,CAACC,gBAAgB;AAC7C,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,OAAO,EAAE,EAAE,CAAE;IACf,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,cAAcA,CAAA,EAAG;MACrB,MAAMC,MAAK,GAAIC,YAAY,CAACC,OAAO,CAAC,QAAQ,CAAC,EAAE;;MAE/C,IAAI,CAAC,IAAI,CAACL,OAAO,CAACM,IAAI,CAAC,CAAC,EAAE;QACxBC,KAAK,CAAC,UAAU,CAAC;QACjB;MACF;MAEA,IAAI;QACF,MAAMC,QAAO,GAAI,MAAMd,KAAK,CAACe,IAAI,CAAC,GAAGd,QAAQ,uBAAuB,EAAE;UACpEe,OAAO,EAAEP,MAAM;UACfH,OAAO,EAAE,IAAI,CAACA;QAChB,CAAC,CAAC;QACFO,KAAK,CAACC,QAAQ,CAACT,IAAI,CAACC,OAAO,CAAC;QAC5B,IAAI,CAACA,OAAM,GAAI,EAAE,EAAE;MACrB,EAAE,OAAOW,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,SAAS,EAAEA,KAAK,CAAC;QAC/BJ,KAAK,CAAC,cAAc,CAAC;MACvB;IACF;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}