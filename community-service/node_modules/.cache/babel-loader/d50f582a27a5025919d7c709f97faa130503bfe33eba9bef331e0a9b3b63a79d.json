{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nimport { handleApiError } from '../utils/errorHandler';\nconst API = process.env.VUE_APP_API_URL;\nexport default {\n  name: 'OrderDetail',\n  data() {\n    return {\n      order: null,\n      loading: true,\n      statusText: {\n        0: '待处理',\n        1: '处理中',\n        2: '已完成',\n        3: '已取消'\n      }\n    };\n  },\n  async created() {\n    await this.fetchOrderDetails();\n  },\n  methods: {\n    getStatusType(status) {\n      const typeMap = {\n        0: 'info',\n        // 待处理\n        1: 'warning',\n        // 处理中\n        2: 'success',\n        // 已完成\n        3: 'danger' // 已取消\n      };\n      return typeMap[status] || 'info';\n    },\n    async fetchOrderDetails() {\n      const orderId = this.$route.params.orderId;\n      try {\n        const response = await axios.get(`${API}/canteen/order/${orderId}`, {\n          headers: {\n            Authorization: `Bearer ${localStorage.getItem('authToken')}`\n          }\n        });\n        const orderData = response.data;\n        if (orderData && orderData.user) {\n          this.order = {\n            ...orderData,\n            email: orderData.user.email,\n            username: orderData.user.username\n          };\n        } else {\n          this.order = orderData;\n        }\n        this.loading = false;\n      } catch (error) {\n        console.error('获取订单详情失败:', error);\n        handleApiError(error);\n      }\n    },\n    formatDate(dateString) {\n      if (!dateString) return '暂无日期';\n      try {\n        const date = new Date(dateString);\n        if (isNaN(date.getTime())) return '无效日期';\n        const options = {\n          year: 'numeric',\n          month: '2-digit',\n          day: '2-digit',\n          hour: '2-digit',\n          minute: '2-digit',\n          hour12: false\n        };\n        return date.toLocaleString('zh-CN', options).replace(/\\//g, '-');\n      } catch (error) {\n        console.error('日期格式化错误:', error);\n        return '日期格式错误';\n      }\n    },\n    goBack() {\n      this.$router.push('/discover');\n    }\n  }\n};","map":{"version":3,"names":["axios","handleApiError","API","process","env","VUE_APP_API_URL","name","data","order","loading","statusText","created","fetchOrderDetails","methods","getStatusType","status","typeMap","orderId","$route","params","response","get","headers","Authorization","localStorage","getItem","orderData","user","email","username","error","console","formatDate","dateString","date","Date","isNaN","getTime","options","year","month","day","hour","minute","hour12","toLocaleString","replace","goBack","$router","push"],"sources":["C:\\Users\\拯救者\\Desktop\\text\\community-service\\src\\components\\OrderDetail.vue"],"sourcesContent":["<template>\n    <div class=\"order-detail\">\n        <el-card>\n            <template #header>\n                <div class=\"card-header\">\n                    <el-page-header @back=\"goBack\" title=\"返回\" content=\"订单详情\" />\n                </div>\n            </template>\n\n            <div v-if=\"order\" class=\"order-content\">\n                <div class=\"info-list\">\n                    <div class=\"info-item\">订单编号：{{ order.id }}</div>\n                    <div class=\"info-item\">用户名：{{ order.username }}</div>\n                    <div class=\"info-item\">邮箱：{{ order.email }}</div>\n                    <div class=\"info-item\">\n                        订单状态：\n                        <el-tag :type=\"getStatusType(order.status)\">\n                            {{ statusText[order.status] || '未知状态' }}\n                        </el-tag>\n                    </div>\n                    <div class=\"info-item\">总价格：<span class=\"price\">¥{{ order.total_price }}</span></div>\n                </div>\n\n                <div v-if=\"order.items && order.items.length\" class=\"order-items\">\n                    <h3>订单商品</h3>\n                    <div class=\"items-list\">\n                        <div v-for=\"item in order.items\" \n                             :key=\"item.menu_item_id\" \n                             class=\"item\">\n                            <div class=\"item-name\">{{ item.name }}</div>\n                            <div class=\"item-info\">\n                                <span>数量：{{ item.quantity }}</span>\n                                <span>单价：¥{{ item.price }}</span>\n                                <span class=\"subtotal\">小计：¥{{ (item.quantity * item.price).toFixed(2) }}</span>\n                            </div>\n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div v-else-if=\"loading\" class=\"loading-wrapper\">\n                <el-empty description=\"正在加载订单信息...\" />\n            </div>\n        </el-card>\n    </div>\n</template>\n\n<script>\nimport axios from 'axios';\nimport { handleApiError } from '../utils/errorHandler';\nconst API = process.env.VUE_APP_API_URL;\n\nexport default {\n    name: 'OrderDetail',\n    data() {\n        return {\n            order: null,\n            loading: true,\n            statusText: {\n                0: '待处理',\n                1: '处理中',\n                2: '已完成',\n                3: '已取消'\n            }\n        };\n    },\n    async created() {\n        await this.fetchOrderDetails();\n    },\n    methods: {\n        getStatusType(status) {\n            const typeMap = {\n                0: 'info',    // 待处理\n                1: 'warning', // 处理中\n                2: 'success', // 已完成\n                3: 'danger'   // 已取消\n            };\n            return typeMap[status] || 'info';\n        },\n        async fetchOrderDetails() {\n            const orderId = this.$route.params.orderId;\n            try {\n                const response = await axios.get(`${API}/canteen/order/${orderId}`, {\n                    headers: {\n                        Authorization: `Bearer ${localStorage.getItem('authToken')}`\n                    }\n                });\n                const orderData = response.data;\n                \n                if (orderData && orderData.user) {\n                    this.order = {\n                        ...orderData,\n                        email: orderData.user.email,\n                        username: orderData.user.username\n                    };\n                } else {\n                    this.order = orderData;\n                }\n                this.loading = false;\n            } catch (error) {\n                console.error('获取订单详情失败:', error);\n                handleApiError(error);\n            }\n        },\n        formatDate(dateString) {\n            if (!dateString) return '暂无日期';\n            try {\n                const date = new Date(dateString);\n                if (isNaN(date.getTime())) return '无效日期';\n                \n                const options = { \n                    year: 'numeric', \n                    month: '2-digit', \n                    day: '2-digit', \n                    hour: '2-digit', \n                    minute: '2-digit',\n                    hour12: false \n                };\n                return date.toLocaleString('zh-CN', options).replace(/\\//g, '-');\n            } catch (error) {\n                console.error('日期格式化错误:', error);\n                return '日期格式错误';\n            }\n        },\n        goBack() {\n            this.$router.push('/discover');\n        }\n    }\n};\n</script>\n\n<style scoped>\n.order-detail {\n    max-width: 800px;\n    margin: 20px auto;\n    padding: 0 20px;\n}\n\n.card-header {\n    display: flex;\n    justify-content: space-between;\n    align-items: center;\n}\n\n.order-content {\n    padding: 20px 0;\n}\n\n.price {\n    color: #f56c6c;\n    font-size: 16px;\n    font-weight: 500;\n}\n\n.order-items {\n    margin-top: 30px;\n}\n\n.order-items h3 {\n    margin-bottom: 20px;\n    font-size: 18px;\n    font-weight: 500;\n    color: #303133;\n}\n\n.info-list {\n    margin-bottom: 20px;\n}\n\n.info-item {\n    margin-bottom: 10px;\n    font-size: 14px;\n    color: #606266;\n}\n\n.items-list {\n    margin-top: 20px;\n}\n\n.item {\n    margin-bottom: 16px;\n    padding: 16px;\n    border: 1px solid #ebeef5;\n    border-radius: 4px;\n    box-shadow: 0 2px 12px 0 rgba(0, 0, 0, 0.1);\n}\n\n.item-name {\n    font-size: 16px;\n    font-weight: 500;\n    color: #303133;\n}\n\n.item-info {\n    margin-top: 10px;\n    font-size: 14px;\n    color: #606266;\n}\n\n.subtotal {\n    color: #409EFF;\n    font-weight: 500;\n}\n\n.loading-wrapper {\n    padding: 40px;\n    text-align: center;\n}\n\n:deep(.el-descriptions) {\n    margin-bottom: 20px;\n}\n\n:deep(.el-descriptions__title) {\n    font-size: 18px;\n    font-weight: 500;\n    margin-bottom: 16px;\n}\n\n:deep(.el-descriptions__label) {\n    font-weight: 500;\n}\n</style>\n"],"mappings":";AAgDA,OAAOA,KAAI,MAAO,OAAO;AACzB,SAASC,cAAa,QAAS,uBAAuB;AACtD,MAAMC,GAAE,GAAIC,OAAO,CAACC,GAAG,CAACC,eAAe;AAEvC,eAAe;EACXC,IAAI,EAAE,aAAa;EACnBC,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,KAAK,EAAE,IAAI;MACXC,OAAO,EAAE,IAAI;MACbC,UAAU,EAAE;QACR,CAAC,EAAE,KAAK;QACR,CAAC,EAAE,KAAK;QACR,CAAC,EAAE,KAAK;QACR,CAAC,EAAE;MACP;IACJ,CAAC;EACL,CAAC;EACD,MAAMC,OAAOA,CAAA,EAAG;IACZ,MAAM,IAAI,CAACC,iBAAiB,CAAC,CAAC;EAClC,CAAC;EACDC,OAAO,EAAE;IACLC,aAAaA,CAACC,MAAM,EAAE;MAClB,MAAMC,OAAM,GAAI;QACZ,CAAC,EAAE,MAAM;QAAK;QACd,CAAC,EAAE,SAAS;QAAE;QACd,CAAC,EAAE,SAAS;QAAE;QACd,CAAC,EAAE,QAAO,CAAI;MAClB,CAAC;MACD,OAAOA,OAAO,CAACD,MAAM,KAAK,MAAM;IACpC,CAAC;IACD,MAAMH,iBAAiBA,CAAA,EAAG;MACtB,MAAMK,OAAM,GAAI,IAAI,CAACC,MAAM,CAACC,MAAM,CAACF,OAAO;MAC1C,IAAI;QACA,MAAMG,QAAO,GAAI,MAAMpB,KAAK,CAACqB,GAAG,CAAC,GAAGnB,GAAG,kBAAkBe,OAAO,EAAE,EAAE;UAChEK,OAAO,EAAE;YACLC,aAAa,EAAE,UAAUC,YAAY,CAACC,OAAO,CAAC,WAAW,CAAC;UAC9D;QACJ,CAAC,CAAC;QACF,MAAMC,SAAQ,GAAIN,QAAQ,CAACb,IAAI;QAE/B,IAAImB,SAAQ,IAAKA,SAAS,CAACC,IAAI,EAAE;UAC7B,IAAI,CAACnB,KAAI,GAAI;YACT,GAAGkB,SAAS;YACZE,KAAK,EAAEF,SAAS,CAACC,IAAI,CAACC,KAAK;YAC3BC,QAAQ,EAAEH,SAAS