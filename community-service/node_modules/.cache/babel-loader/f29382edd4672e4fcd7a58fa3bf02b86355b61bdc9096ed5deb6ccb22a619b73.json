{"ast":null,"code":"import axios from 'axios';\nconst BASE_URL = process.env.VUE_APP_BASE_URL;\nexport default {\n  data() {\n    return {\n      feedbacks: [] // 留言数据\n    };\n  },\n  mounted() {\n    this.fetchFeedbacks(); // 获取留言数据\n  },\n  methods: {\n    async fetchFeedbacks() {\n      try {\n        const response = await axios.get(`${BASE_URL}/api/auth/feedback`);\n        this.feedbacks = response.data;\n      } catch (error) {\n        console.error('获取留言失败:', error);\n      }\n    },\n    async replyFeedback(feedbackId) {\n      const feedback = this.feedbacks.find(item => item.id === feedbackId);\n      if (!feedback.admin_reply.trim()) {\n        alert('回复内容不能为空');\n        return;\n      }\n      try {\n        await axios.put(`http://localhost:3000/api/auth/feedback/${feedbackId}/reply`, {\n          admin_reply: feedback.admin_reply\n        });\n        alert('回复成功');\n        this.fetchFeedbacks(); // 刷新留言列表\n      } catch (error) {\n        console.error('回复留言失败:', error);\n        alert('回复失败，请稍后重试');\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","BASE_URL","process","env","VUE_APP_BASE_URL","data","feedbacks","mounted","fetchFeedbacks","methods","response","get","error","console","replyFeedback","feedbackId","feedback","find","item","id","admin_reply","trim","alert","put"],"sources":["C:\\Users\\拯救者\\Desktop\\text\\community-service\\src\\components\\ManageFeedback.vue"],"sourcesContent":["<template>\r\n    <div class=\"manage-feedback\">\r\n      <h2>留言管理</h2>\r\n      <table>\r\n        <thead>\r\n          <tr>\r\n            <th>用户</th>\r\n            <th>留言内容</th>\r\n            <th>管理员回复</th>\r\n            <th>状态</th>\r\n            <th>操作</th>\r\n          </tr>\r\n        </thead>\r\n        <tbody>\r\n          <tr v-for=\"feedback in feedbacks\" :key=\"feedback.id\">\r\n            <td>{{ feedback.username }}</td>\r\n            <td>{{ feedback.message }}</td>\r\n            <td>\r\n              <textarea v-model=\"feedback.admin_reply\" placeholder=\"回复内容\" rows=\"3\"></textarea>\r\n            </td>\r\n            <td>{{ feedback.status }}</td>\r\n            <td>\r\n              <button @click=\"replyFeedback(feedback.id)\">回复</button>\r\n            </td>\r\n          </tr>\r\n        </tbody>\r\n      </table>\r\n    </div>\r\n  </template>\r\n  \r\n  <script>\r\n  import axios from 'axios';\r\n  const BASE_URL = process.env.VUE_APP_BASE_URL;\r\n  export default {\r\n    data() {\r\n      return {\r\n        feedbacks: [], // 留言数据\r\n      };\r\n    },\r\n    mounted() {\r\n      this.fetchFeedbacks(); // 获取留言数据\r\n    },\r\n    methods: {\r\n      async fetchFeedbacks() {\r\n        try {\r\n          const response = await axios.get(`${BASE_URL}/api/auth/feedback`);\r\n          this.feedbacks = response.data;\r\n        } catch (error) {\r\n          console.error('获取留言失败:', error);\r\n        }\r\n      },\r\n  \r\n      async replyFeedback(feedbackId) {\r\n        const feedback = this.feedbacks.find(item => item.id === feedbackId);\r\n        if (!feedback.admin_reply.trim()) {\r\n          alert('回复内容不能为空');\r\n          return;\r\n        }\r\n  \r\n        try {\r\n          await axios.put(`http://localhost:3000/api/auth/feedback/${feedbackId}/reply`, {\r\n            admin_reply: feedback.admin_reply,\r\n          });\r\n          alert('回复成功');\r\n          this.fetchFeedbacks(); // 刷新留言列表\r\n        } catch (error) {\r\n          console.error('回复留言失败:', error);\r\n          alert('回复失败，请稍后重试');\r\n        }\r\n      },\r\n    },\r\n  };\r\n  </script>\r\n  \r\n  <style scoped>\r\n  .manage-feedback {\r\n    padding: 20px;\r\n  }\r\n  \r\n  table {\r\n    width: 100%;\r\n    border-collapse: collapse;\r\n  }\r\n  \r\n  th, td {\r\n    padding: 10px;\r\n    border: 1px solid #ccc;\r\n  }\r\n  \r\n  textarea {\r\n    width: 100%;\r\n  }\r\n  \r\n  button {\r\n    padding: 10px 20px;\r\n    background-color: #4CAF50;\r\n    color: white;\r\n    border: none;\r\n    cursor: pointer;\r\n  }\r\n  \r\n  button:hover {\r\n    background-color: #45a049;\r\n  }\r\n  </style>\r\n  "],"mappings":"AA+BE,OAAOA,KAAI,MAAO,OAAO;AACzB,MAAMC,QAAO,GAAIC,OAAO,CAACC,GAAG,CAACC,gBAAgB;AAC7C,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,SAAS,EAAE,EAAE,CAAE;IACjB,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,cAAc,CAAC,CAAC,EAAE;EACzB,CAAC;EACDC,OAAO,EAAE;IACP,MAAMD,cAAcA,CAAA,EAAG;MACrB,IAAI;QACF,MAAME,QAAO,GAAI,MAAMV,KAAK,CAACW,GAAG,CAAC,GAAGV,QAAQ,oBAAoB,CAAC;QACjE,IAAI,CAACK,SAAQ,GAAII,QAAQ,CAACL,IAAI;MAChC,EAAE,OAAOO,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,SAAS,EAAEA,KAAK,CAAC;MACjC;IACF,CAAC;IAED,MAAME,aAAaA,CAACC,UAAU,EAAE;MAC9B,MAAMC,QAAO,GAAI,IAAI,CAACV,SAAS,CAACW,IAAI,CAACC,IAAG,IAAKA,IAAI,CAACC,EAAC,KAAMJ,UAAU,CAAC;MACpE,IAAI,CAACC,QAAQ,CAACI,WAAW,CAACC,IAAI,CAAC,CAAC,EAAE;QAChCC,KAAK,CAAC,UAAU,CAAC;QACjB;MACF;MAEA,IAAI;QACF,MAAMtB,KAAK,CAACuB,GAAG,CAAC,2CAA2CR,UAAU,QAAQ,EAAE;UAC7EK,WAAW,EAAEJ,QAAQ,CAACI;QACxB,CAAC,CAAC;QACFE,KAAK,CAAC,MAAM,CAAC;QACb,IAAI,CAACd,cAAc,CAAC,CAAC,EAAE;MACzB,EAAE,OAAOI,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,SAAS,EAAEA,KAAK,CAAC;QAC/BU,KAAK,CAAC,YAAY,CAAC;MACrB;IACF;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}