{"ast":null,"code":"import axios from 'axios';\nconst BASE_URL = process.env.VUE_APP_BASE_URL;\nexport default {\n  data() {\n    return {\n      message: '' // 用户输入的留言内容\n    };\n  },\n  methods: {\n    async submitFeedback() {\n      const userId = localStorage.getItem('userId'); // 假设你已经存储了用户 ID\n\n      if (!this.message.trim()) {\n        alert('留言内容不能为空');\n        return;\n      }\n      try {\n        const response = await axios.post(`${BASE_URL}/api/canteen/feedback`, {\n          user_id: userId,\n          message: this.message\n        });\n        alert(response.data.message);\n        this.message = ''; // 清空留言内容\n      } catch (error) {\n        console.error('提交留言失败:', error);\n        alert('提交留言失败，请稍后重试');\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","BASE_URL","process","env","VUE_APP_BASE_URL","data","message","methods","submitFeedback","userId","localStorage","getItem","trim","alert","response","post","user_id","error","console"],"sources":["C:\\Users\\拯救者\\Desktop\\text\\community-service\\src\\components\\SubmitFeedback.vue"],"sourcesContent":["<template>\n    <div class=\"submit-feedback\">\n      <h2>提交您的建议或留言</h2>\n      <textarea v-model=\"message\" placeholder=\"请填写您的留言\" rows=\"4\" required></textarea>\n      <button @click=\"submitFeedback\">提交留言</button>\n    </div>\n  </template>\n  \n  <script>\n  import axios from 'axios';\n  const BASE_URL = process.env.VUE_APP_BASE_URL;\n  export default {\n    data() {\n      return {\n        message: '', // 用户输入的留言内容\n      };\n    },\n    methods: {\n      async submitFeedback() {\n        const userId = localStorage.getItem('userId'); // 假设你已经存储了用户 ID\n  \n        if (!this.message.trim()) {\n          alert('留言内容不能为空');\n          return;\n        }\n  \n        try {\n          const response = await axios.post(`${BASE_URL}/api/canteen/feedback`, {\n            user_id: userId,\n            message: this.message,\n          });\n          alert(response.data.message);\n          this.message = ''; // 清空留言内容\n        } catch (error) {\n          console.error('提交留言失败:', error);\n          alert('提交留言失败，请稍后重试');\n        }\n      },\n    },\n  };\n  </script>\n  \n  <style scoped>\n  .submit-feedback {\n    padding: 20px;\n  }\n  \n  textarea {\n    width: 100%;\n    margin-bottom: 10px;\n    padding: 10px;\n  }\n  \n  button {\n    padding: 10px 20px;\n    background-color: #4CAF50;\n    color: white;\n    border: none;\n    cursor: pointer;\n  }\n  \n  button:hover {\n    background-color: #45a049;\n  }\n  </style>\n  "],"mappings":"AASE,OAAOA,KAAI,MAAO,OAAO;AACzB,MAAMC,QAAO,GAAIC,OAAO,CAACC,GAAG,CAACC,gBAAgB;AAC7C,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,OAAO,EAAE,EAAE,CAAE;IACf,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,cAAcA,CAAA,EAAG;MACrB,MAAMC,MAAK,GAAIC,YAAY,CAACC,OAAO,CAAC,QAAQ,CAAC,EAAE;;MAE/C,IAAI,CAAC,IAAI,CAACL,OAAO,CAACM,IAAI,CAAC,CAAC,EAAE;QACxBC,KAAK,CAAC,UAAU,CAAC;QACjB;MACF;MAEA,IAAI;QACF,MAAMC,QAAO,GAAI,MAAMd,KAAK,CAACe,IAAI,CAAC,GAAGd,QAAQ,uBAAuB,EAAE;UACpEe,OAAO,EAAEP,MAAM;UACfH,OAAO,EAAE,IAAI,CAACA;QAChB,CAAC,CAAC;QACFO,KAAK,CAACC,QAAQ,CAACT,IAAI,CAACC,OAAO,CAAC;QAC5B,IAAI,CAACA,OAAM,GAAI,EAAE,EAAE;MACrB,EAAE,OAAOW,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,SAAS,EAAEA,KAAK,CAAC;QAC/BJ,KAAK,CAAC,cAAc,CAAC;MACvB;IACF;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}