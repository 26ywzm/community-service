{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nexport default {\n  data() {\n    return {\n      menuItems: [],\n      // 菜单数据\n      cart: [] // 购物车\n    };\n  },\n  computed: {\n    totalPrice() {\n      return this.cart.reduce((total, item) => total + item.price * item.quantity, 0);\n    }\n  },\n  mounted() {\n    this.fetchMenuItems();\n  },\n  methods: {\n    async fetchMenuItems() {\n      try {\n        const response = await axios.get('http://localhost:3000/api/auth/canteen/menu');\n        this.menuItems = response.data.map(item => ({\n          ...item,\n          quantity: 1\n        }));\n      } catch (error) {\n        console.error('获取菜单失败:', error);\n      }\n    },\n    addToCart(item) {\n      const cartItem = this.cart.find(cartItem => cartItem.id === item.id);\n      if (cartItem) {\n        cartItem.quantity += item.quantity;\n      } else {\n        this.cart.push({\n          ...item\n        });\n      }\n    },\n    removeFromCart(itemId) {\n      this.cart = this.cart.filter(item => item.id !== itemId);\n    },\n    async checkout() {\n      try {\n        const response = await axios.post('http://localhost:3000/api/auth/canteen/order', {\n          items: this.cart\n        }, {\n          headers: {\n            Authorization: `Bearer ${localStorage.getItem('authToken')}`\n          }\n        });\n        alert(response.data.message || '结账成功！');\n        this.cart = []; // 清空购物车\n      } catch (error) {\n        console.error('结账失败:', error);\n        alert('结账失败，请重试。');\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","data","menuItems","cart","computed","totalPrice","reduce","total","item","price","quantity","mounted","fetchMenuItems","methods","response","get","map","error","console","addToCart","cartItem","find","id","push","removeFromCart","itemId","filter","checkout","post","items","headers","Authorization","localStorage","getItem","alert","message"],"sources":["C:\\Users\\拯救者\\Desktop\\text\\community-service\\src\\components\\CanteenOrder.vue"],"sourcesContent":["<template>\r\n  <div class=\"canteen-order\">\r\n    <h2>社区食堂</h2>\r\n\r\n    <!-- 菜品列表 -->\r\n    <div v-if=\"menuItems.length > 0\">\r\n      <div v-for=\"item in menuItems\" :key=\"item.id\" class=\"menu-item\">\r\n        <img :src=\"item.image_url\" alt=\"菜品图片\" />\r\n        <h3>{{ item.name }}</h3>\r\n        <p>{{ item.description }}</p>\r\n        <p>价格: {{ item.price }} 元</p>\r\n        <input type=\"number\" v-model=\"item.quantity\" min=\"1\" placeholder=\"数量\" />\r\n        <button @click=\"addToCart(item)\">添加到购物车</button>\r\n      </div>\r\n    </div>\r\n\r\n    <div v-else>\r\n      <p>目前没有可用的菜品。</p>\r\n    </div>\r\n\r\n    <!-- 购物车 -->\r\n    <div class=\"cart\" v-if=\"cart.length > 0\">\r\n      <h3>购物车</h3>\r\n      <ul>\r\n        <li v-for=\"item in cart\" :key=\"item.id\">\r\n          {{ item.name }} - 数量: {{ item.quantity }} - 单价: {{ item.price }} 元 - 总价: {{ item.price * item.quantity }} 元\r\n          <button @click=\"removeFromCart(item.id)\">移除</button>\r\n        </li>\r\n      </ul>\r\n      <p>总价: {{ totalPrice }} 元</p>\r\n      <button @click=\"checkout\">结账</button>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      menuItems: [],  // 菜单数据\r\n      cart: []        // 购物车\r\n    };\r\n  },\r\n  computed: {\r\n    totalPrice() {\r\n      return this.cart.reduce((total, item) => total + item.price * item.quantity, 0);\r\n    }\r\n  },\r\n  mounted() {\r\n    this.fetchMenuItems();\r\n  },\r\n  methods: {\r\n    async fetchMenuItems() {\r\n      try {\r\n        const response = await axios.get('http://localhost:3000/api/auth/canteen/menu');\r\n        this.menuItems = response.data.map(item => ({ ...item, quantity: 1 }));\r\n      } catch (error) {\r\n        console.error('获取菜单失败:', error);\r\n      }\r\n    },\r\n    addToCart(item) {\r\n      const cartItem = this.cart.find(cartItem => cartItem.id === item.id);\r\n      if (cartItem) {\r\n        cartItem.quantity += item.quantity;\r\n      } else {\r\n        this.cart.push({ ...item });\r\n      }\r\n    },\r\n    removeFromCart(itemId) {\r\n      this.cart = this.cart.filter(item => item.id !== itemId);\r\n    },\r\n    async checkout() {\r\n      try {\r\n        const response = await axios.post(\r\n          'http://localhost:3000/api/auth/canteen/order',\r\n          { items: this.cart },\r\n          {\r\n            headers: {\r\n              Authorization: `Bearer ${localStorage.getItem('authToken')}`,\r\n            }\r\n          }\r\n        );\r\n        alert(response.data.message || '结账成功！');\r\n        this.cart = []; // 清空购物车\r\n      } catch (error) {\r\n        console.error('结账失败:', error);\r\n        alert('结账失败，请重试。');\r\n      }\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.canteen-order {\r\n  padding: 20px;\r\n}\r\n\r\n.menu-item {\r\n  border: 1px solid #ccc;\r\n  padding: 10px;\r\n  margin-bottom: 20px;\r\n}\r\n\r\nimg {\r\n  max-width: 100px;\r\n  margin-right: 20px;\r\n}\r\n\r\nbutton {\r\n  background-color: #4CAF50;\r\n  color: white;\r\n  border: none;\r\n  padding: 10px;\r\n  cursor: pointer;\r\n  border-radius: 5px;\r\n}\r\n\r\nbutton:hover {\r\n  background-color: #45a049;\r\n}\r\n\r\n.cart {\r\n  margin-top: 20px;\r\n  border-top: 2px solid #ccc;\r\n  padding-top: 20px;\r\n}\r\n</style>\r\n"],"mappings":";AAoCA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,SAAS,EAAE,EAAE;MAAG;MAChBC,IAAI,EAAE,EAAC,CAAS;IAClB,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,UAAUA,CAAA,EAAG;MACX,OAAO,IAAI,CAACF,IAAI,CAACG,MAAM,CAAC,CAACC,KAAK,EAAEC,IAAI,KAAKD,KAAI,GAAIC,IAAI,CAACC,KAAI,GAAID,IAAI,CAACE,QAAQ,EAAE,CAAC,CAAC;IACjF;EACF,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,cAAc,CAAC,CAAC;EACvB,CAAC;EACDC,OAAO,EAAE;IACP,MAAMD,cAAcA,CAAA,EAAG;MACrB,IAAI;QACF,MAAME,QAAO,GAAI,MAAMd,KAAK,CAACe,GAAG,CAAC,6CAA6C,