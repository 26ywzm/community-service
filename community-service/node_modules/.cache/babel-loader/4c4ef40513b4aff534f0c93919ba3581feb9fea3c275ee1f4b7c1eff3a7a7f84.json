{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nconst API = process.env.VUE_APP_BASE_URL;\nexport default {\n  data() {\n    return {\n      votes: [],\n      loading: false\n    };\n  },\n  async created() {\n    // 检查管理员权限\n    const userRole = localStorage.getItem('userRole');\n    if (userRole !== 'admin') {\n      this.$router.push('/');\n      return;\n    }\n    await this.fetchVotes();\n  },\n  methods: {\n    async fetchVotes() {\n      this.loading = true;\n      try {\n        const token = localStorage.getItem('token');\n        const response = await axios.get(`${API}/api/canteen/votes`, {\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n        this.votes = response.data;\n      } catch (error) {\n        console.error('获取投票失败:', error);\n        this.$message.error('获取投票列表失败');\n      } finally {\n        this.loading = false;\n      }\n    },\n    confirmDelete(vote) {\n      if (confirm(`确定要删除投票\"${vote.title}\"吗？此操作不可撤销。`)) {\n        this.deleteVote(vote.id);\n      }\n    },\n    async deleteVote(voteId) {\n      this.loading = true;\n      try {\n        const token = localStorage.getItem('token');\n        await axios.delete(`${API}/api/canteen/votes/${voteId}`, {\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n        this.$message.success('投票删除成功');\n        await this.fetchVotes();\n      } catch (error) {\n        console.error('删除投票失败:', error);\n        this.$message.error('删除投票失败');\n      } finally {\n        this.loading = false;\n      }\n    },\n    createNewVote() {\n      this.$router.push('/create-vote');\n    }\n  }\n};","map":{"version":3,"names":["axios","API","process","env","VUE_APP_BASE_URL","data","votes","loading","created","userRole","localStorage","getItem","$router","push","fetchVotes","methods","token","response","get","headers","Authorization","error","console","$message","confirmDelete","vote","confirm","title","deleteVote","id","voteId","delete","success","createNewVote"],"sources":["C:\\Users\\拯救者\\Desktop\\text\\community-service\\src\\components\\AdminManageVotes.vue"],"sourcesContent":["<template>\n  <div>\n    <h2>管理投票</h2>\n    <div v-if=\"loading\" class=\"loading\">\n      <span>加载中...</span>\n    </div>\n    <div v-else>\n      <div v-for=\"vote in votes\" :key=\"vote.id\" class=\"vote-item\">\n        <div class=\"vote-info\">\n          <h3>{{ vote.title }}</h3>\n          <p>{{ vote.description }}</p>\n        </div>\n        <div class=\"vote-actions\">\n          <button class=\"delete-btn\" @click=\"confirmDelete(vote)\">删除投票</button>\n        </div>\n      </div>\n      <button class=\"create-btn\" @click=\"createNewVote\">创建新投票</button>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios';\nconst API = process.env.VUE_APP_BASE_URL;\n\nexport default {\n  data() {\n    return {\n      votes: [],\n      loading: false,\n    };\n  },\n  async created() {\n    // 检查管理员权限\n    const userRole = localStorage.getItem('userRole');\n    if (userRole !== 'admin') {\n      this.$router.push('/');\n      return;\n    }\n    await this.fetchVotes();\n  },\n  methods: {\n    async fetchVotes() {\n      this.loading = true;\n      try {\n        const token = localStorage.getItem('token');\n        const response = await axios.get(`${API}/api/canteen/votes`, {\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n        this.votes = response.data;\n      } catch (error) {\n        console.error('获取投票失败:', error);\n        this.$message.error('获取投票列表失败');\n      } finally {\n        this.loading = false;\n      }\n    },\n\n    confirmDelete(vote) {\n      if (confirm(`确定要删除投票\"${vote.title}\"吗？此操作不可撤销。`)) {\n        this.deleteVote(vote.id);\n      }\n    },\n\n    async deleteVote(voteId) {\n      this.loading = true;\n      try {\n        const token = localStorage.getItem('token');\n        await axios.delete(`${API}/api/canteen/votes/${voteId}`, {\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n        this.$message.success('投票删除成功');\n        await this.fetchVotes();\n      } catch (error) {\n        console.error('删除投票失败:', error);\n        this.$message.error('删除投票失败');\n      } finally {\n        this.loading = false;\n      }\n    },\n\n    createNewVote() {\n      this.$router.push('/create-vote');\n    },\n  },\n};\n</script>\n\n<style scoped>\n.loading {\n  text-align: center;\n  padding: 20px;\n  color: #666;\n}\n\n.vote-item {\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  padding: 15px;\n  margin-bottom: 15px;\n  border: 1px solid #e8e8e8;\n  border-radius: 4px;\n  background-color: #fff;\n  transition: all 0.3s;\n}\n\n.vote-item:hover {\n  box-shadow: 0 2px 8px rgba(0,0,0,0.09);\n}\n\n.vote-info {\n  flex: 1;\n}\n\n.vote-info h3 {\n  margin: 0 0 8px 0;\n  color: #333;\n}\n\n.vote-info p {\n  margin: 0;\n  color: #666;\n}\n\n.vote-actions {\n  margin-left: 20px;\n}\n\n.delete-btn {\n  padding: 6px 15px;\n  background-color: #ff4d4f;\n  color: white;\n  border: none;\n  border-radius: 4px;\n  cursor: pointer;\n  transition: all 0.3s;\n}\n\n.delete-btn:hover {\n  background-color: #ff7875;\n}\n\n.create-btn {\n  margin-top: 20px;\n  padding: 8px 20px;\n  background-color: #1890ff;\n  color: white;\n  border: none;\n  border-radius: 4px;\n  cursor: pointer;\n  transition: all 0.3s;\n}\n\n.create-btn:hover {\n  background-color: #40a9ff;\n}\n</style>"],"mappings":";AAsBA,OAAOA,KAAI,MAAO,OAAO;AACzB,MAAMC,GAAE,GAAIC,OAAO,CAACC,GAAG,CAACC,gBAAgB;AAExC,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,OAAO,EAAE;IACX,CAAC;EACH,CAAC;EACD,MAAMC,OAAOA,CAAA,EAAG;IACd;IACA,MAAMC,QAAO,GAAIC,YAAY,CAACC,OAAO,CAAC,UAAU,CAAC;IACjD,IAAIF,QAAO,KAAM,OAAO,EAAE;MACxB,IAAI,CAACG,OAAO,CAACC,IAAI,CAAC,GAAG,CAAC;MACtB;IACF;IACA,MAAM,IAAI,CAACC,UAAU,CAAC,CAAC;