{"ast":null,"code":"import axios from 'axios';\nexport default {\n  data() {\n    return {\n      menuItems: [],\n      newItem: {\n        name: '',\n        price: '',\n        image_url: '',\n        description: ''\n      },\n      isEditing: false // 编辑状态\n    };\n  },\n  mounted() {\n    this.fetchMenuItems(); // 加载菜品数据\n  },\n  methods: {\n    async fetchMenuItems() {\n      try {\n        const response = await axios.get('http://localhost:3000/api/auth/canteen/menu');\n        this.menuItems = response.data;\n      } catch (error) {\n        console.error('获取菜单失败:', error);\n      }\n    },\n    async addMenuItem() {\n      try {\n        await axios.post('http://localhost:3000/api/auth/canteen/menu', this.newItem);\n        this.fetchMenuItems();\n        this.resetForm();\n      } catch (error) {\n        console.error('添加菜品失败:', error);\n      }\n    },\n    async deleteMenuItem(itemId) {\n      try {\n        await axios.delete(`http://localhost:3000/api/auth/canteen/menu/${itemId}`);\n        this.fetchMenuItems();\n      } catch (error) {\n        console.error('删除菜品失败:', error);\n      }\n    },\n    editMenuItem(item) {\n      this.newItem = {\n        ...item\n      }; // 复制要编辑的菜品\n      this.isEditing = true; // 设置为编辑状态\n    },\n    async updateMenuItem() {\n      try {\n        await axios.put(`http://localhost:3000/api/auth/canteen/menu/${this.newItem.id}`, this.newItem);\n        this.fetchMenuItems(); // 刷新菜单列表\n        this.resetForm(); // 清空表单\n      } catch (error) {\n        console.error('更新菜品失败:', error);\n      }\n    },\n    cancelEdit() {\n      this.resetForm(); // 清空表单\n    },\n    resetForm() {\n      this.isEditing = false; // 退出编辑状态\n      this.newItem = {\n        name: '',\n        price: '',\n        image_url: '',\n        description: ''\n      }; // 清空表单\n    }\n  }\n};","map":{"version":3,"names":["axios","data","menuItems","newItem","name","price","image_url","description","isEditing","mounted","fetchMenuItems","methods","response","get","error","console","addMenuItem","post","resetForm","deleteMenuItem","itemId","delete","editMenuItem","item","updateMenuItem","put","id","cancelEdit"],"sources":["C:\\Users\\拯救者\\Desktop\\text\\community-service\\src\\components\\CanteenAdmin.vue"],"sourcesContent":["<template>\r\n  <div class=\"canteen-admin\">\r\n    <h2>食堂管理</h2>\r\n  \r\n    <!-- 添加/编辑菜品的表单 -->\r\n    <div>\r\n      <h3>{{ isEditing ? '编辑菜品' : '添加新菜品' }}</h3>\r\n      <form @submit.prevent=\"isEditing ? updateMenuItem() : addMenuItem()\">\r\n        <div>\r\n          <label>菜品名称</label>\r\n          <input type=\"text\" v-model=\"newItem.name\" required />\r\n        </div>\r\n        <div>\r\n          <label>价格</label>\r\n          <input type=\"number\" v-model=\"newItem.price\" required />\r\n        </div>\r\n        <div>\r\n          <label>图片链接</label>\r\n          <input type=\"text\" v-model=\"newItem.image_url\" />\r\n        </div>\r\n        <div>\r\n          <label>描述</label>\r\n          <textarea v-model=\"newItem.description\"></textarea>\r\n        </div>\r\n        <button type=\"submit\">{{ isEditing ? '更新菜品' : '添加菜品' }}</button>\r\n        <button type=\"button\" @click=\"cancelEdit\" v-if=\"isEditing\">取消</button>\r\n      </form>\r\n    </div>\r\n  \r\n    <!-- 菜品列表 -->\r\n    <h3>菜品列表</h3>\r\n    <div v-if=\"menuItems.length > 0\">\r\n      <div v-for=\"item in menuItems\" :key=\"item.id\" class=\"menu-item\">\r\n        <img :src=\"item.image_url\" alt=\"菜品图片\" />\r\n        <h3>{{ item.name }}</h3>\r\n        <p>价格: {{ item.price }} 元</p>\r\n        <p>描述: {{ item.description }}</p>\r\n        <button @click=\"deleteMenuItem(item.id)\">删除</button>\r\n        <button @click=\"editMenuItem(item)\">编辑</button>\r\n      </div>\r\n    </div>\r\n    <div v-else>\r\n      <p>目前没有可用的菜品。</p>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      menuItems: [],\r\n      newItem: { name: '', price: '', image_url: '', description: '' },\r\n      isEditing: false, // 编辑状态\r\n    };\r\n  },\r\n  mounted() {\r\n    this.fetchMenuItems(); // 加载菜品数据\r\n  },\r\n  methods: {\r\n    async fetchMenuItems() {\r\n      try {\r\n        const response = await axios.get('http://localhost:3000/api/auth/canteen/menu');\r\n        this.menuItems = response.data;\r\n      } catch (error) {\r\n        console.error('获取菜单失败:', error);\r\n      }\r\n    },\r\n    async addMenuItem() {\r\n      try {\r\n        await axios.post('http://localhost:3000/api/auth/canteen/menu', this.newItem);\r\n        this.fetchMenuItems();\r\n        this.resetForm();\r\n      } catch (error) {\r\n        console.error('添加菜品失败:', error);\r\n      }\r\n    },\r\n    async deleteMenuItem(itemId) {\r\n      try {\r\n        await axios.delete(`http://localhost:3000/api/auth/canteen/menu/${itemId}`);\r\n        this.fetchMenuItems();\r\n      } catch (error) {\r\n        console.error('删除菜品失败:', error);\r\n      }\r\n    },\r\n    editMenuItem(item) {\r\n      this.newItem = { ...item }; // 复制要编辑的菜品\r\n      this.isEditing = true; // 设置为编辑状态\r\n    },\r\n    async updateMenuItem() {\r\n      try {\r\n        await axios.put(`http://localhost:3000/api/auth/canteen/menu/${this.newItem.id}`, this.newItem);\r\n        this.fetchMenuItems(); // 刷新菜单列表\r\n        this.resetForm(); // 清空表单\r\n      } catch (error) {\r\n        console.error('更新菜品失败:', error);\r\n      }\r\n    },\r\n    cancelEdit() {\r\n      this.resetForm(); // 清空表单\r\n    },\r\n    resetForm() {\r\n      this.isEditing = false; // 退出编辑状态\r\n      this.newItem = { name: '', price: '', image_url: '', description: '' }; // 清空表单\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n  \r\n  <style scoped>\r\n  .canteen-admin {\r\n    padding: 20px;\r\n  }\r\n  \r\n  .menu-item {\r\n    border: 1px solid #ccc;\r\n    padding: 10px;\r\n    margin-bottom: 20px;\r\n  }\r\n  \r\n  img {\r\n    max-width: 100px;\r\n    margin-right: 20px;\r\n  }\r\n  \r\n  button {\r\n    background-color: #4CAF50;\r\n    color: white;\r\n    border: none;\r\n    padding: 10px;\r\n    cursor: pointer;\r\n    border-radius: 5px;\r\n  }\r\n  \r\n  button:hover {\r\n    background-color: #45a049;\r\n  }\r\n  </style>\r\n  "],"mappings":"AAgDA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,SAAS,EAAE,EAAE;MACbC,OAAO,EAAE;QAAEC,IAAI,EAAE,EAAE;QAAEC,KAAK,EAAE,EAAE;QAAEC,SAAS,EAAE,EAAE;QAAEC,WAAW,EAAE;MAAG,CAAC;MAChEC,SAAS,EAAE,KAAK,CAAE;IACpB,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,cAAc,CAAC,CAAC,EAAE;EACzB,CAAC;EACDC,OAAO,EAAE;IACP,MAAMD,cAAcA,CAAA,EAAG;MACrB,IAAI;QACF,MAAME,QAAO,GAAI,MAAMZ,KAAK,CAACa,GAAG,CAAC,6CAA6C,CAAC;QAC/E,IAAI,CAACX,SAAQ,GAAIU,QAAQ,CAACX,IAAI;MAChC,EAAE,OAAOa,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,SAAS,EAAEA,KAAK,CAAC;MACjC;IACF,CAAC;IACD,MAAME,WAAWA,CAAA,EAAG;MAClB,IAAI;QACF,MAAMhB,KAAK,CAACiB,IAAI,CAAC,6CAA6C,EAAE,IAAI,CAACd,OAAO,CAAC;QAC7E,IAAI,CAACO,cAAc,CAAC,CAAC;QACrB,IAAI,CAACQ,SAAS,CAAC,CAAC;MAClB,EAAE,OAAOJ,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,SAAS,EAAEA,KAAK,CAAC;MACjC;IACF,CAAC;IACD,MAAMK,cAAcA,CAACC,MAAM,EAAE;MAC3B,IAAI;QACF,MAAMpB,KAAK,CAACqB,MAAM,CAAC,+CAA+CD,MAAM,EAAE,CAAC;QAC3E,IAAI,CAACV,cAAc,CAAC,CAAC;MACvB,EAAE,OAAOI,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,SAAS,EAAEA,KAAK,CAAC;MACjC;IACF,CAAC;IACDQ,YAAYA,CAACC,IAAI,EAAE;MACjB,IAAI,CAACpB,OAAM,GAAI;QAAE,GAAGoB;MAAK,CAAC,EAAE;MAC5B,IAAI,CAACf,SAAQ,GAAI,IAAI,EAAE;IACzB,CAAC;IACD,MAAMgB,cAAcA,CAAA,EAAG;MACrB,IAAI;QACF,MAAMxB,KAAK,CAACyB,GAAG,CAAC,+CAA+C,IAAI,CAACtB,OAAO,CAACuB,EAAE,EAAE,EAAE,IAAI,CAACvB,OAAO,CAAC;QAC/F,IAAI,CAACO,cAAc,CAAC,CAAC,EAAE;QACvB,IAAI,CAACQ,SAAS,CAAC,CAAC,EAAE;MACpB,EAAE,OAAOJ,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,SAAS,EAAEA,KAAK,CAAC;MACjC;IACF,CAAC;IACDa,UAAUA,CAAA,EAAG;MACX,IAAI,CAACT,SAAS,CAAC,CAAC,EAAE;IACpB,CAAC;IACDA,SAASA,CAAA,EAAG;MACV,IAAI,CAACV,SAAQ,GAAI,KAAK,EAAE;MACxB,IAAI,CAACL,OAAM,GAAI;QAAEC,IAAI,EAAE,EAAE;QAAEC,KAAK,EAAE,EAAE;QAAEC,SAAS,EAAE,EAAE;QAAEC,WA