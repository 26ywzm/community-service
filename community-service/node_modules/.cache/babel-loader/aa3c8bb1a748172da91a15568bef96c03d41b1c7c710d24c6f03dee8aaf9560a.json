{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from \"axios\";\n// import { verifyAuth } from \"../utils/auth\";\nimport { handleApiError } from \"../utils/errorHandler\";\nconst API = process.env.VUE_APP_API_URL;\nexport default {\n  data() {\n    return {\n      showAdminModal: false,\n      users: [],\n      admins: [],\n      orders: [],\n      verificationInterval: null\n    };\n  },\n  computed: {\n    isLoggedIn() {\n      return !!localStorage.getItem(\"authToken\");\n    },\n    isAdmin() {\n      const role = localStorage.getItem(\"userRole\");\n      return role === \"admin\";\n    },\n    isSuperAdmin() {\n      const role = localStorage.getItem(\"userRole\");\n      return role === \"super_admin\";\n    }\n  },\n  created() {\n    if (this.isLoggedIn) {\n      this.startVerification();\n    }\n  },\n  beforeUnmount() {\n    if (this.verificationInterval) {\n      clearInterval(this.verificationInterval);\n    }\n  },\n  methods: {\n    toggleAdminModal() {\n      this.showAdminModal = !this.showAdminModal;\n    },\n    async fetchUserOrders() {\n      try {\n        const response = await axios.get(`${API}/orders/user`, {\n          headers: {\n            Authorization: `Bearer ${localStorage.getItem(\"authToken\")}`\n          }\n        });\n        this.orders = response.data;\n      } catch (error) {\n        handleApiError(error);\n      }\n    },\n    async fetchUsers() {\n      try {\n        const response = await axios.get(`${API}/users`, {\n          headers: {\n            Authorization: `Bearer ${localStorage.getItem(\"authToken\")}`\n          }\n        });\n        this.users = response.data.users;\n      } catch (error) {\n        handleApiError(error);\n      }\n    },\n    async fetchAdmins() {\n      try {\n        const response = await axios.get(`${API}/admins`, {\n          headers: {\n            Authorization: `Bearer ${localStorage.getItem(\"authToken\")}`\n          }\n        });\n        this.admins = response.data.admins;\n      } catch (error) {\n        handleApiError(error);\n      }\n    },\n    async promoteUser(userId) {\n      if (this.isSuperAdmin) {\n        await axios.post(`${API}/promote/${userId}`, {}, {\n          headers: {\n            Authorization: `Bearer ${localStorage.getItem(\"authToken\")}`\n          }\n        });\n        this.fetchUsers();\n        this.fetchAdmins();\n      }\n    },\n    async demoteUser(adminId) {\n      if (this.isSuperAdmin) {\n        await axios.post(`${API}/demote/${adminId}`, {}, {\n          headers: {\n            Authorization: `Bearer ${localStorage.getItem(\"authToken\")}`\n          }\n        });\n        this.fetchUsers();\n        this.fetchAdmins();\n      }\n    },\n    async deleteUser(userId) {\n      if (this.isSuperAdmin) {\n        try {\n          await axios.delete(`${API}/users/${userId}`, {\n            headers: {\n              Authorization: `Bearer ${localStorage.getItem(\"authToken\")}`\n            }\n          });\n          this.fetchUsers();\n          this.fetchAdmins();\n        } catch (error) {\n          handleApiError(error);\n        }\n      }\n    },\n    viewOrderDetails(orderId) {\n      this.$router.push({\n        name: \"OrderDetail\",\n        params: {\n          orderId\n        }\n      });\n    },\n    formatDate(dateString) {\n      const options = {\n        year: \"numeric\",\n        month: \"long\",\n        day: \"numeric\",\n        hour: \"2-digit\",\n        minute: \"2-digit\"\n      };\n      return new Date(dateString).toLocaleDateString(\"zh-CN\", options);\n    }\n  },\n  mounted() {\n    if (this.isAdmin || this.isSuperAdmin) {\n      this.fetchUsers();\n      this.fetchAdmins();\n    }\n  }\n};","map":{"version":3,"names":["axios","handleApiError","API","process","env","VUE_APP_API_URL","data","showAdminModal","users","admins","orders","verificationInterval","computed","isLoggedIn","localStorage","getItem","isAdmin","role","isSuperAdmin","created","startVerification","beforeUnmount","clearInterval","methods","toggleAdminModal","fetchUserOrders","response","get","headers","Authorization","error","fetchUsers","fetchAdmins","promoteUser","userId","post","demoteUser","adminId","deleteUser","delete","viewOrderDetails","orderId","$router","push","name","params","formatDate","dateString","options","year","month","day","hour","minute","Date","toLocaleDateString","mounted"],"sources":["C:\\Users\\拯救者\\Desktop\\text\\community-service\\src\\components\\DiscoverPage.vue"],"sourcesContent":["<template>\r\n  <div class=\"discover\">\r\n    <h2 class=\"title\">发现</h2>\r\n\r\n    <!-- 检查用户是否已登录 -->\r\n    <div v-if=\"isLoggedIn\">\r\n      <!-- 普通用户的功能 -->\r\n      <div v-if=\"!isAdmin && !isSuperAdmin\" class=\"button-group\">\r\n        <router-link to=\"/CanteenOrder\">\r\n          <button class=\"action-button\">社区食堂</button>\r\n        </router-link>\r\n        <button @click=\"fetchUserOrders\" class=\"action-button\">查看我的订单</button>\r\n      </div>\r\n\r\n      <!-- 用户订单列表 -->\r\n      <div v-if=\"orders.length > 0\">\r\n        <h3>我的订单</h3>\r\n        <ul class=\"list\">\r\n          <li v-for=\"order in orders\" :key=\"order.id\">\r\n            订单 ID: {{ order.id }}, 总价格: {{ order.total_price }} 元, 创建时间: {{ formatDate(order.created_at) }}\r\n            <button @click=\"viewOrderDetails(order.id)\" class=\"action-button\">查看详情</button>\r\n          </li>\r\n        </ul>\r\n      </div>\r\n\r\n      <!-- 管理员和超级管理员的功能 -->\r\n      <div v-if=\"isAdmin || isSuperAdmin\">\r\n        <div class=\"button-group\">\r\n          <router-link to=\"/articles/new\">\r\n            <button class=\"admin-button\">文章编写</button>\r\n          </router-link>\r\n          <button @click=\"toggleAdminModal\" class=\"admin-button\">\r\n            管理模块\r\n          </button>\r\n        </div>\r\n\r\n        <!-- 弹出框 -->\r\n        <div v-if=\"showAdminModal\" class=\"modal-backdrop\" @click=\"toggleAdminModal\">\r\n          <div class=\"modal-content\" @click.stop>\r\n            <h3 class=\"modal-title\">管理员和用户列表</h3>\r\n            <button class=\"close-button\" @click=\"toggleAdminModal\">关闭</button>\r\n            <div>\r\n              <h4>管理员列表</h4>\r\n              <ul class=\"list\">\r\n                <li v-for=\"admin in admins\" :key=\"admin.id\">\r\n                  {{ admin.username }}\r\n                  <span v-if=\"isSuperAdmin\">\r\n                    <button @click=\"demoteUser(admin.id)\" class=\"action-button\">降为用户</button>\r\n                    <button @click=\"deleteUser(admin.id)\" class=\"close-button\">删除</button>\r\n                  </span>\r\n                </li>\r\n              </ul>\r\n            </div>\r\n            <div>\r\n              <h4>用户列表</h4>\r\n              <ul class=\"list\">\r\n                <li v-for=\"user in users\" :key=\"user.id\">\r\n                  {{ user.username }}\r\n                  <span v-if=\"isSuperAdmin\">\r\n                    <button @click=\"promoteUser(user.id)\" class=\"action-button\">升为管理员</button>\r\n                    <button @click=\"deleteUser(user.id)\" class=\"close-button\">删除</button>\r\n                  </span>\r\n                </li>\r\n              </ul>\r\n            </div>\r\n          </div>\r\n        </div>\r\n\r\n        <!-- 其他管理员功能 -->\r\n        <div class=\"button-group\">\r\n          <router-link to=\"/canteenadmin\">\r\n            <button class=\"admin-button\">食堂管理</button>\r\n          </router-link>\r\n          <router-link to=\"/canteen/orders\">\r\n            <button class=\"admin-button\">订单管理</button>\r\n          </router-link>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- 未登录用户 -->\r\n    <div v-else>\r\n      <p>请登录以查看内容。</p>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\n// import { verifyAuth } from \"../utils/auth\";\r\nimport { handleApiError } from \"../utils/errorHandler\";\r\nconst API = process.env.VUE_APP_API_URL;\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      showAdminModal: false,\r\n      users: [],\r\n      admins: [],\r\n      orders: [],\r\n      verificationInterval: null,\r\n    };\r\n  },\r\n  computed: {\r\n    isLoggedIn() {\r\n      return !!localStorage.getItem(\"authToken\");\r\n    },\r\n    isAdmin() {\r\n      const role = localStorage.getItem(\"userRole\");\r\n      return role === \"admin\";\r\n    },\r\n    isSuperAdmin() {\r\n      const role = localStorage.getItem(\"userRole\");\r\n      return role === \"super_admin\";\r\n    },\r\n  },\r\n  created() {\r\n    if (this.isLoggedIn) {\r\n      this.startVerification();\r\n    }\r\n  },\r\n  beforeUnmount() {\r\n    if (this.verificationInterval) {\r\n      clearInterval(this.verificationInterval);\r\n    }\r\n  },\r\n  methods: {\r\n    toggleAdminModal() {\r\n      this.showAdminModal = !this.showAdminModal;\r\n    },\r\n    async fetchUserOrders() {\r\n      try {\r\n        const response = await axios.get(`${API}/orders/user`, {\r\n          headers: { Authorization: `Bearer ${localStorage.getItem(\"authToken\")}` },\r\n        });\r\n        this.orders = response.data;\r\n      } catch (error) {\r\n        handleApiError(error);\r\n      }\r\n    },\r\n    async fetchUsers() {\r\n      try {\r\n        const response = await axios.get(`${API}/users`, {\r\n          headers: { Authorization: `Bearer ${localStorage.getItem(\"authToken\")}` },\r\n        });\r\n        this.users = response.data.users;\r\n      } catch (error) {\r\n        handleApiError(error);\r\n      }\r\n    },\r\n    async fetchAdmins() {\r\n      try {\r\n        const response = await axios.get(`${API}/admins`, {\r\n          headers: { Authorization: `Bearer ${localStorage.getItem(\"authToken\")}` },\r\n        });\r\n        this.admins = response.data.admins;\r\n      } catch (error) {\r\n        handleApiError(error);\r\n      }\r\n    },\r\n    async promoteUser(userId) {\r\n      if (this.isSuperAdmin) {\r\n        await axios.post(`${API}/promote/${userId}`, {}, { headers: { Authorization: `Bearer ${localStorage.getItem(\"authToken\")}` } });\r\n        this.fetchUsers();\r\n        this.fetchAdmins();\r\n      }\r\n    },\r\n    async demoteUser(adminId) {\r\n      if (this.isSuperAdmin) {\r\n        await axios.post(`${API}/demote/${adminId}`, {}, { headers: { Authorization: `Bearer ${localStorage.getItem(\"authToken\")}` } });\r\n        this.fetchUsers();\r\n        this.fetchAdmins();\r\n      }\r\n    },\r\n    async deleteUser(userId) {\r\n      if (this.isSuperAdmin) {\r\n        try {\r\n          await axios.delete(`${API}/users/${userId}`, { headers: { Authorization: `Bearer ${localStorage.getItem(\"authToken\")}` } });\r\n          this.fetchUsers();\r\n          this.fetchAdmins();\r\n        } catch (error) {\r\n          handleApiError(error);\r\n        }\r\n      }\r\n    },\r\n    viewOrderDetails(orderId) {\r\n      this.$router.push({ name: \"OrderDetail\", params: { orderId } });\r\n    },\r\n    formatDate(dateString) {\r\n      const options = { year: \"numeric\", month: \"long\", day: \"numeric\", hour: \"2-digit\", minute: \"2-digit\" };\r\n      return new Date(dateString).toLocaleDateString(\"zh-CN\", options);\r\n    },\r\n  },\r\n  mounted() {\r\n    if (this.isAdmin || this.isSuperAdmin) {\r\n      this.fetchUsers();\r\n      this.fetchAdmins();\r\n    }\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n/* 弹出框样式 */\r\n.modal-backdrop {\r\n  position: fixed;\r\n  top: 0;\r\n  left: 0;\r\n  width: 100vw;\r\n  height: 100vh;\r\n  background: rgba(0, 0, 0, 0.5);\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  z-index: 1000;\r\n}\r\n\r\n.modal-content {\r\n  background: white;\r\n  padding: 20px;\r\n  border-radius: 8px;\r\n  width: 50%;\r\n  position: relative;\r\n}\r\n\r\n.modal-title {\r\n  font-size: 20px;\r\n  margin-bottom: 10px;\r\n}\r\n\r\n.close-button {\r\n  position: absolute;\r\n  top: 10px;\r\n  right: 10px;\r\n  background: red;\r\n  color: white;\r\n  border: none;\r\n  border-radius: 50%;\r\n  padding: 5px 10px;\r\n  cursor: pointer;\r\n}\r\n\r\n.close-button:hover {\r\n  background: darkred;\r\n}\r\n\r\n/* 其他样式 */\r\n.discover {\r\n  padding: 20px;\r\n  background-color: #f9f9f9;\r\n  border-radius: 8px;\r\n}\r\n.button-group {\r\n  text-align: center;\r\n  margin-bottom: 15px;\r\n}\r\n</style>\r\n"],"mappings":";AAwFA,OAAOA,KAAI,MAAO,OAAO;AACzB;AACA,SAASC,cAAa,QAAS,uBAAuB;AACtD,MAAMC,GAAE,GAAIC,OAAO,CAACC,GAAG,CAACC,eAAe;AAEvC,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,cAAc,EAAE,KAAK;MACrBC,KAAK,EAAE,EAAE;MACTC,MAAM,EAAE,EAAE;MACVC,MAAM,EAAE,EAAE;MACVC,oBAAoB,EAAE;IACxB,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,UAAUA,CAAA,EAAG;MACX,OAAO,CAAC,CAACC,YAAY,CAACC,OAAO,CAAC,WAAW,CAAC;IAC5C,CAAC;IACDC,OAAOA,CAAA,EAAG;MACR,MAAMC,IAAG,GAAIH,YAAY,CAACC,OAAO,CAAC,UAAU,CAAC;MAC7C,OAAOE,IAAG,KAAM,OAAO;IACzB,CAAC;IACDC,YAAYA,CAAA,EAAG;MACb,MAAMD,IAAG,GAAIH,YAAY,CAACC,OAAO,CAAC,UAAU,CAAC;MAC7C,OAAOE,IAAG,KAAM,aAAa;IAC/B;EACF,CAAC;EACDE,OAAOA,CAAA,EAAG;IACR,IAAI,IAAI,CAACN,UAAU,EAAE;MACnB,IAAI,CAACO,iBAAiB,CAAC,CAAC;IAC1B;EACF,CAAC;EACDC,aAAaA,CAAA,EAAG;IACd,IAAI,IAAI,CAACV,oBAAoB,EAAE;MAC7BW,aAAa,CAAC,IAAI,CAACX,oBAAoB,CAAC;IAC1C;EACF,CAAC;EACDY,OAAO,EAAE;IACPC,gBAAgBA,CAAA,EAAG;MACjB,IAAI,CAACjB,cAAa,GAAI,CAAC,IAAI,CAACA,cAAc;IAC5C,CAAC;IACD,MAAMkB,eAAeA,CAAA,EAAG;MACtB,IAAI;QACF,MAAMC,QAAO,GAAI,MAAM1B,KAAK,CAAC2B,GAAG,CAAC,GAAGzB,GAAG,cAAc,EAAE;UACrD0B,OAAO,EAAE;YAAEC,aAAa,EAAE,UAAUf,YAAY,CAACC,OAAO,CAAC,WAAW,CAAC;UAAG;QAC1E,CAAC,CAAC;QACF,IAAI,CAACL,MAAK,GAAIgB,QAAQ,CAACpB,IAAI;MAC7B,EAAE,OAAOwB,KAAK,EAAE;QACd7B,cAAc,CAAC6B,KAAK,CAAC;MACvB;IACF,CAAC;IACD,MAAMC,UAAUA,CAAA,EAAG;MACjB,IAAI;QACF,MAAML,QAAO,GAAI,MAAM1B,KAAK,CAAC2B,GAAG,CAAC,GAAGzB,GAAG,QAAQ,EAAE;UAC/C0B,OAAO,EAAE;YAAEC,aAAa,EAAE,UAAUf,YAAY,CAACC,OAAO,CAAC,WAAW,CAAC;UAAG;QAC1E,CAAC,CAAC;QACF,IAAI,CAACP,KAAI,GAAIkB,QAAQ,CAACpB,IAAI,CAACE,KAAK;MAClC,EAAE,OAAOsB,KAAK,EAAE;QACd7B,cAAc