v-model=\"currentTab\" active-color=\"#1e90ff\" inactive-color=\"#999\">\n      <van-tabbar-item to=\"/\">\n        <template #icon>\n          <van-icon name=\"home-o\" size=\"24px\" />\n        </template>\n        首页\n      </van-tabbar-item>\n      <template v-if=\"isLoggedIn\">\n        <van-tabbar-item to=\"/discover\">\n          <template #icon>\n            <van-icon name=\"apps-o\" size=\"24px\" />\n          </template>\n          发现\n        </van-tabbar-item>\n        <van-tabbar-item to=\"/profile\">\n          <template #icon>\n            <van-icon name=\"user-o\" size=\"24px\" />\n          </template>\n          我的\n        </van-tabbar-item>\n      </template>\n      <template v-else>\n        <van-tabbar-item to=\"/login\">\n          <template #icon>\n            <van-icon name=\"user-o\" size=\"24px\" />\n          </template>\n          登录\n        </van-tabbar-item>\n      </template>\n    </van-tabbar>\n  </div>\n</template>\n\n<script>\nimport { logout } from './utils/auth';\n\nexport default {\n  data() {\n    return {\n      isMobile: false,\n      currentTab: 0,\n      isNavHidden: false,\n      lastScrollTop: 0,\n      activeIndex: '1'\n    };\n  },\n  computed: {\n    isLoggedIn() {\n      return !!localStorage.getItem('userId');\n    }\n  },\n  mounted() {\n    this.checkDevice();\n    window.addEventListener('resize', this.checkDevice);\n    window.addEventListener('scroll', this.handleScroll);\n    \n    // 根据当前路由设置激活的菜单项\n    this.setActiveIndex();\n  },\n  watch: {\n    // 监听路由变化\n    $route: {\n      immediate: true,\n      handler(to) {\n        this.setActiveIndex();\n        \n        // 根据路由更新底部导航当前项\n        if (to.path === '/') this.currentTab = 0;\n        else if (to.path === '/discover') this.currentTab = 1;\n        else if (to.path === '/profile' || to.path === '/login') this.currentTab = 2;\n      }\n    }\n  },\n  methods: {\n    checkDevice() {\n      this.isMobile = window.innerWidth <= 768;\n    },\n    handleScroll() {\n      const st = window.pageYOffset || document.documentElement.scrollTop;\n      this.isNavHidden = st > this.lastScrollTop && st > 100;\n      this.lastScrollTop = st;\n    },\n    setActiveIndex() {\n      const path = this.$route.path;\n      if (path === '/') this.activeIndex = '1';\n      else if (path === '/discover') this.activeIndex = '2';\n      else if (path === '/profile') this.activeIndex = '3';\n      else if (path === '/login') this.activeIndex = '2';\n      else if (path === '/register') this.activeIndex = '3';\n    },\n    async handleLogout() {\n      try {\n        await logout();\n        this.$router.push('/login');\n        this.$message.success('退出登录成功');\n      } catch (error) {\n        console.error('退出登录失败:', error);\n        this.$message.error('退出登录失败');\n      }\n    }\n  },\n  beforeUnmount() {\n    window.removeEventListener('resize', this.checkDevice);\n    window.removeEventListener('scroll', this.handleScroll);\n  }\n};\n</script>\n\n<style>\n.top-menu {\n  position: fixed;\n  top: 0;\n  left: 0;\n  right: 0;\n  z-index: 1000;\n  transition: transform 0.3s;\n  display: flex;\n  justify-content: space-between;\n  padding: 0 20px;\n}\n\n.nav-hidden {\n  transform: translateY(-100%);\n}\n\n.logo {\n  font-size: 20px;\n  color: #fff;\n  line-height: 60px;\n  margin-right: 20px;\n}\n\n.menu-right {\n  display: flex;\n  flex: 1;\n  justify-content: flex-end;\n}\n\n.content-padding {\n  padding-top: 60px;\n}\n\n/* 移动端样式 */\n.van-tabbar {\n  z-index: 1000;\n}\n\n/* 路由链接样式 */\n.el-menu-item a {\n  text-decoration: none;\n  color: inherit;\n}\n</style>"],"mappings":";AAwEA,SAASA,MAAK,QAAS,cAAc;AAErC,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,KAAK;MACfC,UAAU,EAAE,CAAC;MACbC,WAAW,EAAE,KAAK;MAClBC,aAAa,EAAE,CAAC;MAChBC,WAAW,EAAE;IACf,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,UAAUA,CAAA,EAAG;MACX,OAAO,CAAC,CAACC,YAAY,CAACC,OAAO,CAAC,QAAQ,CAAC;IACzC;EACF,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,WAAW,CAAC,CAAC;IAClBC,MAAM,CAACC,gBAA