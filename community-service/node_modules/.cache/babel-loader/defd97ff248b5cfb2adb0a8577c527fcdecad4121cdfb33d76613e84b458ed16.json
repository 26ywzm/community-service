{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { logout } from './utils/auth';\nexport default {\n  data() {\n    return {\n      isMobile: false,\n      currentTab: 0,\n      isNavHidden: false,\n      lastScrollTop: 0,\n      activeIndex: '1'\n    };\n  },\n  computed: {\n    isLoggedIn() {\n      return !!localStorage.getItem('userId');\n    }\n  },\n  mounted() {\n    this.checkDevice();\n    window.addEventListener('resize', this.checkDevice);\n    window.addEventListener('scroll', this.handleScroll);\n\n    // 根据当前路由设置激活的菜单项\n    this.setActiveIndex();\n  },\n  watch: {\n    // 监听路由变化\n    $route: {\n      immediate: true,\n      handler(to) {\n        this.setActiveIndex();\n\n        // 根据路由更新底部导航当前项\n        if (to.path === '/') this.currentTab = 0;else if (to.path === '/discover') this.currentTab = 1;else if (to.path === '/profile' || to.path === '/login') this.currentTab = 2;\n      }\n    }\n  },\n  methods: {\n    checkDevice() {\n      this.isMobile = window.innerWidth <= 768;\n    },\n    handleScroll() {\n      const st = window.pageYOffset || document.documentElement.scrollTop;\n      this.isNavHidden = st > this.lastScrollTop && st > 100;\n      this.lastScrollTop = st;\n    },\n    setActiveIndex() {\n      const path = this.$route.path;\n      if (path === '/') this.activeIndex = '1';else if (path === '/discover') this.activeIndex = '2';else if (path === '/profile') this.activeIndex = '3';else if (path === '/login') this.activeIndex = '2';else if (path === '/register') this.activeIndex = '3';\n    },\n    async handleLogout() {\n      try {\n        await logout();\n        this.$router.push('/login');\n        this.$message.success('退出登录成功');\n      } catch (error) {\n        console.error('退出登录失败:', error);\n        this.$message.error('退出登录失败');\n      }\n    }\n  },\n  beforeUnmount() {\n    window.removeEventListener('resize', this.checkDevice);\n    window.removeEventListener('scroll', this.handleScroll);\n  }\n};"