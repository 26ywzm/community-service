{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nexport default {\n  data() {\n    return {\n      order: null // 存储订单详情\n    };\n  },\n  created() {\n    const orderId = this.$route.params.orderId; // 从路由获取订单 ID\n    console.log('获取的 orderId:', orderId); // 打印 orderId\n    if (orderId && orderId !== ':orderId') {\n      this.fetchOrderDetails(orderId); // 获取订单详细信息\n    } else {\n      alert('订单 ID 无效');\n    }\n  },\n  methods: {\n    async fetchOrderDetails(orderId) {\n      try {\n        const response = await axios.get(`http://localhost:3000/api/auth/canteen/order/${orderId}`, {\n          headers: {\n            Authorization: `Bearer ${localStorage.getItem('authToken')}`\n          }\n        });\n        this.order = response.data; // 保存订单数据\n      } catch (error) {\n        console.error('获取订单详细信息失败:', error);\n        alert('获取订单信息失败，请重试。');\n      }\n    },\n    formatDate(dateString) {\n      const options = {\n        year: 'numeric',\n        month: 'long',\n        day: 'numeric',\n        hour: '2-digit',\n        minute: '2-digit'\n      };\n      return new Date(dateString).toLocaleDateString('zh-CN', options); // 格式化日期\n    },\n    goBack() {\n      this.$router.push('/discover'); // 返回到点餐页面\n    }\n  }\n};","map":{"version":3,"names":["axios","data","order","created","orderId","$route","params","console","log","fetchOrderDetails","alert","methods","response","get","headers","Authorization","localStorage","getItem","error","formatDate","dateString","options","year","month","day","hour","minute","Date","toLocaleDateString","goBack","$router","push"],"sources":["C:\\Users\\拯救者\\Desktop\\text\\community-service\\src\\components\\OrderDetail.vue"],"sourcesContent":["<template>\r\n    <div class=\"order-detail\">\r\n        <h2>订单详细信息</h2>\r\n\r\n        <div v-if=\"order\">\r\n            <h3>订单 ID: {{ order.id }}</h3>\r\n            <p>用户邮箱: {{ order.email }}</p>\r\n            <p>订单状态: {{ order.status }}</p>\r\n            <p>总价格: {{ order.total_price }} 元</p>\r\n            <p>创建时间: {{ formatDate(order.created_at) }}</p>\r\n\r\n            <h4>订单内容:</h4>\r\n            <ul>\r\n                <li v-for=\"detail in order.details\" :key=\"detail.id\">\r\n                    {{ detail.menu_item.name }} ({{ detail.quantity }}): {{ detail.price }} 元\r\n                </li>\r\n            </ul>\r\n        </div>\r\n\r\n        <div v-else>\r\n            <p>正在加载订单信息...</p>\r\n        </div>\r\n\r\n        <button @click=\"goBack\">返回</button>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n    data() {\r\n        return {\r\n            order: null, // 存储订单详情\r\n        };\r\n    },\r\n    created() {\r\n        const orderId = this.$route.params.orderId; // 从路由获取订单 ID\r\n        console.log('获取的 orderId:', orderId); // 打印 orderId\r\n  if (orderId && orderId !== ':orderId') {\r\n    this.fetchOrderDetails(orderId); // 获取订单详细信息\r\n  } else {\r\n    alert('订单 ID 无效');\r\n  }\r\n    },\r\n    methods: {\r\n        async fetchOrderDetails(orderId) {\r\n            try {\r\n                const response = await axios.get(`http://localhost:3000/api/auth/canteen/order/${orderId}`, {\r\n                    headers: {\r\n                        Authorization: `Bearer ${localStorage.getItem('authToken')}`,\r\n                    },\r\n                });\r\n                this.order = response.data; // 保存订单数据\r\n            } catch (error) {\r\n                console.error('获取订单详细信息失败:', error);\r\n                alert('获取订单信息失败，请重试。');\r\n            }\r\n        },\r\n\r\n        formatDate(dateString) {\r\n            const options = { year: 'numeric', month: 'long', day: 'numeric', hour: '2-digit', minute: '2-digit' };\r\n            return new Date(dateString).toLocaleDateString('zh-CN', options); // 格式化日期\r\n        },\r\n\r\n        goBack() {\r\n            this.$router.push('/discover'); // 返回到点餐页面\r\n        }\r\n    }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.order-detail {\r\n    padding: 20px;\r\n}\r\n\r\nbutton {\r\n    background-color: #4CAF50;\r\n    color: white;\r\n    border: none;\r\n    padding: 10px;\r\n    cursor: pointer;\r\n    border-radius: 5px;\r\n}\r\n\r\nbutton:hover {\r\n    background-color: #45a049;\r\n}\r\n</style>"],"mappings":";AA4BA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACXC,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,KAAK,EAAE,IAAI,CAAE;IACjB,CAAC;EACL,CAAC;EACDC,OAAOA,CAAA,EAAG;IACN,MAAMC,OAAM,GAAI,IAAI,CAACC,MAAM,CAACC,MAAM,CAACF,OAAO,EAAE;IAC5CG,OAAO,CAACC,GAAG,CAAC,cAAc,EAAEJ,OAAO,CAAC,EAAE;IAC5C,IAAIA,OAAM,IAAKA,OAAM,KAAM,UAAU,EAAE;MACrC,IAAI,CAACK,iBAAiB,CAACL,OAAO,CAAC,EAAE;IACnC,OAAO;MACLM,KAAK,CAAC,UAAU,CAAC;IACnB;EACE,CAAC;EACDC,OAAO,EAAE;IACL,MAAMF,iBAAiBA,CAACL,OAAO,EAAE;MAC7B,IAAI;QACA,MAAMQ,QAAO,GAAI,MAAMZ,KAAK,CAACa,GAAG,CAAC,gDAAgDT,OAAO,EAAE,EAAE;UACxFU,OAAO,EAAE;YACLC,aAAa,EAAE,UAAUC,YAAY,CAACC,OAAO,CAAC,WAAW,CAAC;UAC9D;QACJ,CAAC,CAAC;QACF,IAAI,CAACf,KAAI,GAAIU,QAAQ,CAACX,IAAI,EAAE;MAChC,EAAE,OAAOiB,KAAK,EAAE;QACZX,OAAO,CAACW,KAAK,CAAC,aAAa,EAAEA,KAAK,CAAC;QACnCR,KAAK,CAAC,eAAe,CAAC;MAC1B;IACJ,CAAC;IAEDS,UAAUA,CAACC,UAAU,EAAE;MACnB,MAAMC,OAAM,GAAI;QAAEC,IAAI,EAAE,SAAS;QAAEC,KAAK,EAAE,MAAM;QAAEC,GAAG,EAAE,SAAS;QAAEC,IAAI,EAAE,SAAS;QAAEC,MAAM,EAAE;MAAU,CAAC;MACtG,OAAO,IAAIC,IAAI,CAACP,UAAU,CAAC,CAACQ,kBAAkB,CAAC,OAAO,EAAEP,OAAO,CAAC,EAAE;IACtE,CAAC;IAEDQ,MAAMA,CAAA,EAAG;MACL,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,WAAW,CAAC,EAAE;IACpC;EACJ;AACJ,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}