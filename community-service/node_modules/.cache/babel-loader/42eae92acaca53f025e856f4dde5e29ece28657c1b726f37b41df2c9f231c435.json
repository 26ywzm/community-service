{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nexport default {\n  data() {\n    return {\n      menuItems: [],\n      cart: [] // 存储用户的购物车\n    };\n  },\n  mounted() {\n    this.fetchMenuItems(); // 加载可用的菜单项\n  },\n  methods: {\n    async fetchMenuItems() {\n      try {\n        const response = await axios.get('http://localhost:3000/api/auth/canteen/menu');\n        this.menuItems = response.data.map(item => ({\n          ...item,\n          quantity: 1\n        })); // 添加 quantity 字段并设置默认值为1\n      } catch (error) {\n        console.error('获取菜单失败:', error);\n      }\n    },\n    async order(item) {\n      if (item.quantity <= 0) {\n        alert('订购数量必须大于0');\n        return;\n      }\n      this.cart.push({\n        id: item.id,\n        quantity: item.quantity\n      }); // 将菜品添加到购物车\n      alert('已添加到购物车');\n    },\n    async checkout() {\n      try {\n        const response = await axios.post('http://localhost:3000/api/auth/canteen/order', {\n          items: this.cart\n        }, {\n          headers: {\n            Authorization: `Bearer ${localStorage.getItem('authToken')}` // Bearer 格式\n          }\n        });\n        alert(response.data.message || '结账成功！');\n        const orderId = response.data.orderId; // 从响应中获取订单 ID\n        this.cart = []; // 清空购物车\n        this.$router.push(`/order-detail/${orderId}`); // 跳转到订单详情页面\n      } catch (error) {\n        console.error('结账失败:', error);\n        alert('结账失败，请重试。');\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","data","menuItems","cart","mounted","fetchMenuItems","methods","response","get","map","item","quantity","error","console","order","alert","push","id","checkout","post","items","headers","Authorization","localStorage","getItem","message","orderId","$router"],"sources":["C:\\Users\\拯救者\\Desktop\\text\\community-service\\src\\components\\CanteenOrder.vue"],"sourcesContent":["<template>\r\n  <div class=\"canteen-order\">\r\n    <h2>社区食堂</h2>\r\n    <!-- 菜品列表 -->\r\n    <div v-if=\"menuItems.length > 0\">\r\n      <div v-for=\"item in menuItems\" :key=\"item.id\" class=\"menu-item\">\r\n        <img :src=\"item.image_url\" alt=\"菜品图片\" />\r\n        <h3>{{ item.name }}</h3>\r\n        <p>{{ item.description }}</p>\r\n        <p>价格: {{ item.price }} 元</p>\r\n\r\n        <input type=\"number\" v-model=\"item.quantity\" min=\"1\" placeholder=\"订购数量\" />\r\n\r\n        <button @click=\"order(item)\">订餐</button>\r\n      </div>\r\n      <button @click=\"checkout\">结账</button> <!-- 添加结账按钮 -->\r\n    </div>\r\n    <div v-else>\r\n      <p>目前没有可用的菜品。</p>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      menuItems: [],\r\n      cart: [], // 存储用户的购物车\r\n    };\r\n  },\r\n  mounted() {\r\n    this.fetchMenuItems(); // 加载可用的菜单项\r\n  },\r\n  methods: {\r\n    async fetchMenuItems() {\r\n      try {\r\n        const response = await axios.get('http://localhost:3000/api/auth/canteen/menu');\r\n        this.menuItems = response.data.map(item => ({ ...item, quantity: 1 })); // 添加 quantity 字段并设置默认值为1\r\n      } catch (error) {\r\n        console.error('获取菜单失败:', error);\r\n      }\r\n    },\r\n\r\n    async order(item) {\r\n      if (item.quantity <= 0) {\r\n        alert('订购数量必须大于0');\r\n        return;\r\n      }\r\n      this.cart.push({ id: item.id, quantity: item.quantity }); // 将菜品添加到购物车\r\n      alert('已添加到购物车');\r\n    },\r\n\r\n    async checkout() {\r\n      try {\r\n        const response = await axios.post(\r\n          'http://localhost:3000/api/auth/canteen/order',\r\n          { items: this.cart },\r\n          {\r\n            headers: {\r\n              Authorization: `Bearer ${localStorage.getItem('authToken')}`, // Bearer 格式\r\n            }\r\n          }\r\n        );\r\n        alert(response.data.message || '结账成功！');\r\n\r\n        const orderId = response.data.orderId; // 从响应中获取订单 ID\r\n        this.cart = []; // 清空购物车\r\n        this.$router.push(`/order-detail/${orderId}`); // 跳转到订单详情页面\r\n      } catch (error) {\r\n        console.error('结账失败:', error);\r\n        alert('结账失败，请重试。');\r\n      }\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.canteen-order {\r\n  padding: 20px;\r\n}\r\n\r\n.menu-item {\r\n  border: 1px solid #ccc;\r\n  padding: 10px;\r\n  margin-bottom: 20px;\r\n}\r\n\r\nimg {\r\n  max-width: 100px;\r\n  margin-right: 20px;\r\n}\r\n\r\nbutton {\r\n  background-color: #4CAF50;\r\n  color: white;\r\n  border: none;\r\n  padding: 10px;\r\n  cursor: pointer;\r\n  border-radius: 5px;\r\n}\r\n\r\nbutton:hover {\r\n  background-color: #45a049;\r\n}\r\n\r\n.cart {\r\n  margin-top: 20px;\r\n  border-top: 2px solid #ccc;\r\n  padding-top: 20px;\r\n}\r\n</style>\r\n"],"mappings":";AAwBA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,SAAS,EAAE,EAAE;MACbC,IAAI,EAAE,EAAE,CAAE;IACZ,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,cAAc,CAAC,CAAC,EAAE;EACzB,CAAC;EACDC,OAAO,EAAE;IACP,MAAMD,cAAcA,CAAA,EAAG;MACrB,IAAI;QACF,MAAME,QAAO,GAAI,MAAMP,KAAK,CAACQ,GAAG,CAAC,6CAA6C,CAAC;QAC/E,IAAI,CAACN,SAAQ,GAAIK,QAAQ,CAACN,IAAI,CAACQ,GAAG,CAACC,IAAG,KAAM;UAAE,GAAGA,IAAI;UAAEC,QAAQ,EAAE;QAAE,CAAC,CAAC,CAAC,EAAE;MAC1E,EAAE,OAAOC,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,SAAS,EAAEA,KAAK,CAAC;MACjC;IACF,CAAC;IAED,MAAME,KAAKA,CAACJ,IAAI,EAAE;MAChB,IAAIA,IAAI,CAACC,QAAO,IAAK,CAAC,EAAE;QACtBI,KAAK,CAAC,WAAW,CAAC;QAClB;MACF;MACA,IAAI,CAACZ,IAAI,CAACa,IAAI,CAAC;QAAEC,EAAE,EAAEP,IAAI,CAACO,EAAE;QAAEN,QAAQ,EAAED,IAAI,CAACC;MAAS,CAAC,CAAC,EAAE;MAC1DI,KAAK,CAAC,SAAS,CAAC;IAClB,CAAC;IAED,MAAMG,QAAQA,CAAA,EAAG;MACf,IAAI;QACF,MAAMX,QAAO,GAAI,MAAMP,KAAK,CAACmB,IAAI,CAC/B,8CAA8C,EAC9C;UAAEC,KAAK,EAAE,IAAI,CAACjB;QAAK,CAAC,EACpB;UACEkB,OAAO,EAAE;YACPC,aAAa,EAAE,UAAUC,YAAY,CAACC,OAAO,CAAC,WAAW,CAAC,EAAE,CAAE;UAChE;QACF,CACF,CAAC;QACDT,KAAK,CAACR,QAAQ,CAACN,IAAI,CAACwB,OAAM,IAAK,OAAO,CAAC;QAEvC,MAAMC,OAAM,GAAInB,QAAQ,CAACN,IAAI,CAACyB,OAAO,EAAE;QACvC,IAAI